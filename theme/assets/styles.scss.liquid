/*=== tooltip ===*/
.swatch .tooltip {
    background: $body-second-color;
    color: $body-general-color;
    &:after {
        border-top: solid $body-second-color 5px;
    }
}
/*=== product-simple-tab ===*/
.product-table {
    margin: 55px auto 0;
}
.product-simple-tab {
