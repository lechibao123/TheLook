                            @include margin-end(10px); 
                        }
                        @media (max-width: 767px) and (min-width: 576px) {
                            @include margin-end(5px); 
                            @include padding-start(6px); 
                            @include padding-end(6px); 
                        }
                        &:last-child {
                            @include margin-end(0 !important); 
                        }                        
                    }
                    .countdown-amount {
                        display: block;
                        color: $body-title-color;
                        font-size: $size-h2;
                        padding: 0;
                        text-align: center;   
                        @media (max-width: 479px) {             
                            font-size: $size-h2 - 3px; 
                        }
                        @media (max-width: 767px) and (min-width: 576px) {
                            font-size: $size-h2 - 4px; 
                        }
                    }
                    .countdown-period {
                        padding: 0;
                        display: block;
                        text-align: center;
                        color: $body-title-color;
                        font-size: $body-font-size;   
                        @media (max-width: 767px) and (min-width: 576px) {
                            font-size: $body-font-size - 2px;   
                        }                      
                    }
                }
            }
        }
        .product-detail-review {
            margin: 0 0 15px;
            display: inline-block;
            > a {
                @include margin-start(8px); 
                font-size: 13px;        
                &:hover {
                    color: $main-color;
                }
            }
        }     
        .short-description {
            clear: both;
            margin: 0 0 25px;      
            padding: 0 0 20px;
            line-height: 1.8;   
            color: $body-light-color;   
            border-bottom: 1px solid $body-border-color;
            h5 {
                display: none;
            }    
            ul, ol {
                margin-top: 15px;
                margin-bottom: 15px;
                line-height: 20px;
                list-style: disc outside none;
                @include padding-start(20px);  
                li {
                    margin: 0 0 1px;
                }         
                &:last-child {
                    margin-bottom: 0;
                }
            }            
        }
        .size-chart {
            display: inline-block;
            vertical-align: middle;
            margin: 0;
            @include margin-end(30px); 
            > a {            
                line-height: 20px;
                text-transform: capitalize;                
                color: $body-light-color;
                font-size: $body-font-size - 1px;
                align-items: center; 
                @include flexbox;
                svg {
                    width: 16px;
                    height: 16px;
                    @include margin-end(5px);  
                }
                &:hover {
                    color: $main-color;
                    &:before {
                        color: inherit;
                    }
                }
            }
        }
        .rating-links {     
            margin: 0 0 12px;
            line-height: 20px;      
            overflow: hidden;
            clear: both;
            > a {
                color: $body-light-color;    
                font-size: $body-font-size - 1px;     
                &:hover {
                    color: $main-color;
                }      
            }
            .rating-inner {        
                display: inline-block;   
                float: left;     
                position: relative;
                color: $body-light-color;
                @include margin-end(15px);
            }
        }
        .spr-badge {         
            display: inline-block;   
            float: left;     
            position: relative;
            color: $body-light-color;
            &:after {
                content: '';
                width: 1px;
                height: 12px;
                color: $body-light-color;
                @include ab(4px, n, n, -8px);
            }
            .spr-badge-caption {
                float: left;    
                display: inline-block;    
                font-size: $body-font-size - 1px;          
                &:before {
                    content: "(";
                }
                &:after {
                    content: ")";
                }
            }      
        }   
        .price-preview-container {
            overflow: hidden;
        }
        .group-wlcp {
            overflow: hidden;
            margin: 0;            
        }
        .secure-image {
            overflow: hidden;
            margin: 25px auto;
            clear: both;
            padding: 20px 0 0;
            border-top: 1px solid $body-border-color; 
            @media (max-width: 767px) {
                margin-bottom: 0;
            }
        }
        .product-vendor {
            font-size: $body-font-size - 2px;     
            margin: 0 0 3px;
            a {
                color: $body-light-color;
                &:hover {
                    color: $main-color;
                }
            }
        }
        #stock {      
            clear: both;                         
            line-height: 16px;   
            border-bottom: 1px solid $body-border-color;
            margin: 0 0 20px;  
            padding: 0 0 18px; 
            font-size: $body-font-size - 1px;
            > span {                          
                color: $body-light-color;   
                &.stock {
                    color: #5bb300;
                    font-weight: 700;
                }     
            }      
        }    
        .sold-out {
            pointer-events: none;
            float: left;      
            @include opacity(0.65);
        }
        .group-price-sale {
            overflow: hidden;
            margin: 0 0 20px; 
            display: table;
            vertical-align: middle;
            width: 100%;
            > div {
                float: none;
                display: inline-block;
                vertical-align: middle;
            }    
        }
        .sale-detail {
            float: left;
            border: 2px solid $body-label-sale-background;
            color: $body-label-sale-background;
            text-transform: uppercase;
            font-size: $body-font-size;
            padding: 0 15px;
            line-height: 26px;
            @media (max-width: 479px) {
                padding: 0 10px;
            }
        }
        .detail-price {
            overflow: hidden;
            margin: 20px 0 0;
        }
        .product-price {        
            text-align: start;                          
            padding: 0;
            margin: 0 0 25px;      
            .price-sale {           
                float: left;   
                font-size: $size-h1 - 1px;     
                color: $body-title-color;  
                @include margin-end(20px); 
            }
            .price {                    
                text-align: start;
                padding: 0;
                margin: 0;        
                border: none;                                             
                font-size: $size-h1 - 1px;     
            }
            .price-compare { 
                margin: 15px 0 0;        
                display: inline-block;                   
                font-size: $body-font-size;         
            }
        }    
        .custom-field {
            color: $main-color;
            margin: 0 0 20px;
        }
        .group-cw {
            position: relative;
            display: block;
            width: 100%;
            margin: 0;   
            clear: both;   
            @include swatch-product; 
        } 
        .qty-add-cart {      
            margin: 18px 0 0; 
            overflow: hidden;  
        }
        .pre-order {      
            margin: 0 0 10px;
            @media (max-width: 575px) {
                display: block;
                width: 100%;
                float: none;
                clear: both;
                .btn-pre-order {
                    width: 100%;
                }
            } 
            .btn-pre-order {
                line-height: 30px;
            }      
        }  
        .pre-order-success {
            padding: 10px 0 7px;      
            color: #5bb300;
            font-weight: 600;
            clear: both; 
            @include flex(0 0 100%); 
        }
        .action-button {   
            list-style: none;  
            float: left;              
            margin: 0 0 15px;   
            @include margin-end(15px);   
            @media (max-width: 575px) {
                width: 100%;
                display: block; 
                .btn {
                    display: block;
                    width: 100%;
                }           
            }
            .btn {
                margin: 0;     
                min-width: 40px;
                line-height: 26px;
                padding: 10px 40px;        
                font-size: $body-font-size; 
                color: $white;          
                .demo-icon {
                    font-size: 18px;
                    margin: 0;
                    @include margin-end(7px); 
                }     
                &.disabled {
                    position: relative;         
                    color: transparent !important;
                    @include opacity(1);
                    .demo-icon {
                        @include opacity(0);
                    }
                    &:after {
                        content: '\e839';   
                        font-family: $font-family-icon;                 
                        font-size: $font-icon;
                        color: $white;
                        @include ab(10px, 50%, n, n); 
                        @include margin-start(-13px);
                        -webkit-animation: fa-spin 2s infinite linear;
                        animation: fa-spin 2s infinite linear;   
                    }
                }  
                &.disabled[disabled="disabled"] {
                    color: $button-btn1-txt !important;       
                    @include opacity(0.65); 
                    &:after {
                        content: none;
                    }
                }
                &:hover {
                    color: $button-hover-btn1-txt;
                }
            } 
        }
        .shopify-payment-button {
            margin: 0 0 15px; 
            float: left; 
            @media (max-width: 575px) {
                float: none;
                width: 100%;
                display: block;
            }     
            button {
                font-weight: 700;
                line-height: 26px;
                text-transform: capitalize;
                background: $button-hover-btn1;  
                color: $button-hover-btn1-txt;
                padding: 12px 15px;
                font-size: $body-font-size;  
                @include border-radius-25;
                @include transition(all 0.35s ease 0s); 
                &:hover {
                    background: $button-btn1;  
                    color: $button-btn1-txt;
                }
                &.shopify-payment-button__more-options {          
                    @include border-radius-25;
                }
            }
            .shopify-payment-button__button--hidden {
                height: 0;
                margin: 0 auto;
                padding: 0;
            }
        }
        .add-to-wishlist,
        .add-to-compare {        
            display: inline-block;
            vertical-align: middle;  
            color: $body-light-color;  
            font-size: $body-font-size - 1px;   
            svg {
                float: left;  
                @include margin-end(5px);   
            }
            &:hover {
                color: $main-color;  
                svg {
                    fill: $main-color !important;
                }  
            }       
        }  
        #widget-fave-html {
            display: inline-block;
            .ssw-fave-btn-content {
                border: none;
                padding: 0; 
                font-size: $body-font-size - 1px; 
                color: $body-light-color;
                display: inline-block;
                vertical-align: middle;
                line-height: normal;  
                &:hover {
                    color: $main-color;  
                } 
                &:focus, &:active {
                    outline: none;
                    @include box-shadow(none);
                }     
            }    
            i { 
                font-size: 16px; 
                display: inline-block;
                color: inherit; 
                margin: 0; 
                @include margin-end(3px);   
                &:before {
                    margin: 0;
                }
            } 
            .ssw-fave-btn-counter {
                display: none;
            }
        }
        .add-to-wishlist, #widget-fave-html {  
            @include margin-end(30px);
        }
        .product-action {     
            .variants-wrapper {
                margin: 5px 0 0;
                clear: both;       
                .selector-wrapper {
                    margin-bottom: 20px; 
                    clear: both;
                    text-align: start;    
                    display: none;     
                    select {
                        min-width: 50%;
                        height: 34px;
                        padding: 0 15px;
                        line-height: 32px;            
                        font-size: $body-font-size;        
                        color: $body-general-color;                                                      
                    }          
                }
                label {    
                    color: $body-title-color;             
                    font-size: $body-font-size;
                    margin: 0;          
                    float: left;
                    line-height: 34px;
                    min-width: 110px;
                    white-space: nowrap;
                    padding: 0;
                    @include padding-end(15px); 
                }
            } 
            .wrap-quantity-add-cart {
                list-style: none; 
                padding: 0;
                margin: 0;      
            }     
            .quantity-product {
                width: 140px;
                margin: 0 0 15px;  
                @include margin-end(30px);    
                @media (min-width: 1440px) {
                    float: left;
                }      
                @media (max-width: 767px) and (min-width: 576px) {
                    width: 186px;
                }   
                @media (max-width: 575px) {   
                    width: 186px;
                }                   
                label {          
                    margin: -6px 0 0;          
                    line-height: 22px;  
                    font-size: $body-font-size - 1px;        
                    color: $body-title-color;        
                    display: block;                          
                }                            
                input[type=number]::-webkit-inner-spin-button,
                input[type=number]::-webkit-outer-spin-button {
                    -webkit-appearance: none;    
                }               
                .qty-stock {
                    display: inline-block;
                    margin-bottom: 15px;
                } 
            }          
            .total-price {        
                color: $body-price-color;
                display: inline-block;
                margin-top: 16px;
                @include margin-end(10px);
                .price-sale{
                    @include margin-end(10px);
                }
            }     
        }    
    }      
}
.templateProduct { 
    .breadcrumb {
        margin: 0;
        @media (min-width: 992px) {
            @include padding-end(100px); 
        }
    }      
    .nav-tabs li a {
        padding-bottom: 15px;
    }
    .tab-content {
        clear: both;
        padding: 4.05% 6%;          
        margin: -8px auto 0;        
        border: 1px solid $body-border-color;
        @include border-radius(15px, 15px, 15px, 15px);    
        @media (max-width: 767px) and (min-width: 480px){
            padding: 5%;
        }
        @media (max-width: 479px) {
            padding: 20px;
        }
        #tabs-description {
            line-height: 22px;
        }
        .tab-pane {
            display: block;
            height: 0;
            @include opacity(0);
            &.show {
                height: auto;
                position: relative;
                @include opacity(1);
            }
        }   
        table {
            border-collapse: collapse;
            margin: -8px auto 23px;
            th {      
                padding: 7px 30px; 
                background: $body-second-color;
                color: $body-title-color;
                text-transform: uppercase;
            }
            td {
                border-bottom: 1px solid $body-border-color;
                padding: 7px 30px;
                text-align: start;  
                line-height: 1.5;           
            }
            tbody tr {
                &:first-child td {
                    border-top: none;
                    padding-top: 0;
                }
                &:last-child td {
                    border-bottom: none;
                }
            }
        } 
        .table-extended {
            margin-top: 30px;
            td {
                text-align: start;
                padding: 0;
                width: 50%;
                border: none;
                @media (max-width: 767px) {
                    width: 100%;
                    display: block;
                }
                img {
                    max-width: 100%;
                    @media (max-width: 767px) {
                        margin-bottom: 15px;
                    }
                }
            }
            &.align-left td img {
                position: relative;
                right: -8%;    
                float: right; 
                @media (max-width: 767px) {
                    right: auto;
                }        
            }
            &.align-right td img {
                position: relative;
                left: -8%;   
                @media (max-width: 767px) {
                    left: auto; 
                }         
            }
        }
        iframe {max-width: 100%; }
        h5 { 
            color: $body-title-color;      
        }
        h1, h2, h3, h4, h5, h6 {
            margin: 0 0 23px;     
            font-weight: 500;
        }
        p {          
            margin-bottom: 40px;
            @media (max-width: 767px) {
                margin-bottom: 20px;
            }
            &:last-of-type {
                margin-bottom: 0;
            }
        }   
        .video {
            display: block;
            position: relative;
            width: 100%;
            height: 0;
            padding-bottom: 56.25%;
            iframe {
                @include ab(0, 0, n, n);
                width: 100%;
                height: 100%;
            }
        }
        .bundled-items {
            overflow: hidden;   
            #crust-items {
                @media (min-width: 992px) {
                    float: left;
                    width: 75%;
                    @include padding-end(15px); 
                }
            }
            .action-bundle {
                @media (min-width: 992px) {
                    float: left;
                    width: 25%;
                    @include padding-start(15px); 
                }
                @media (max-width: 991px) and (min-width: 576px) {
                    float: right;
                }
                @media (max-width: 575px) { 
                    margin-top: 20px;
                }
                .total-price {
                    margin: 0 0 15px;
                    .price {
                        color: $body-price-sale-color;  
                        font-size: $size-h3 + 4px;
                    }
                }
                .btn {
                    padding-top: 4px;
                    padding-bottom: 4px;
                }
            }
            .bundled-images {
                padding: 0;
                list-style-type: none;
                overflow: hidden;
                li {
                    position: relative;
                    padding: 20px 25px;
                    overflow: visible;
                    float: left;
                    &:not(:first-child) {
                        &:after {
                            content: '';
                            height: 70%;
                            width: 1px;
                            background: $body-border-color;
                            @include ab(50%, 0, n, n);
                            @include translate(0, -50%);
                        }
                        &:before {
                            content: '+';
                            height: 32px;
                            width: 32px;
                            line-height: 30px;
                            text-align: center;
                            border: 1px solid $body-border-color;
                            background: $body-background-color;
                            color: $body-general-color;
                            font-size: $size-h4;
                            z-index: 2;
                            @include ab(50%, -16px, n, n);
                            @include translate(0, -50%);
                            @include border-radius-circle;
                        }
                    }
                    @media (min-width: 576px) {            
                        width: 33.333333%;
                    }
                    @media (max-width: 575px) {            
                        width: 50%;
                        @include padding-start(20px); 
                        @include padding-end(20px); 
                        &:nth-child(3) {
                            clear: both;
                            width: 100%;
                            padding-top: 30px;
                            @include padding-start(25%);  
                            &:after {
                                height: 1px;
                                width: 200%;
                                left: -25%;
                                right: -25%;
                                top: 0;                
                                @include translate(0, 0);
                            }
                            &:before {
                                left: 50%;
                                top: -16px;
                                right: auto;    
                                @include translate(-50%, 0);             
                            }
                        }
                    }
                    h5 {
                        color: $body-title-product;     
                        font-size: $product-font-size;      
                        font-weight: 700;
                        margin: 0 0 15px; 
                        display: -webkit-box;
                        display: -ms-box;
                        -webkit-line-clamp: 2;
                        -webkit-box-orient: vertical;
                        line-height: 18px;
                        height: 36px;   
                        overflow: hidden;
                        @media (max-width: 479px) {
                            font-size: $product-font-size - 2px;      
                        }    
                        a {
                            color: $body-title-product;
                            &:hover {
                                text-decoration: underline;
                            }                              
                        } 
                    }
                    img {
                        display: block;
                        margin: 0 0 10px;
                    }
                    .price {
                        display: block;
                    }
                    &:first-child .price {
                        color: $body-light-color;
                    } 
                }
            }  
            .wrap-bundled-item {
                overflow: hidden;
            }
            .bundled-item {
                position: relative;
                line-height: 20px;
                float: left;       
                margin: 0 0 7px;
                padding: 0;
                @include padding-start(20px);  
                input[type="checkbox"] {
                    @include ab(0, 0, n, n);
                    @include opacity(0);
                    margin: 0;
                    width: 100%;
                    height: 100%;     
                    z-index: 2;     
                }   
                .wrap-item,
                > .b-info {
                    position: relative;
                    &:before {
                        content: '';
                        width: 14px;
                        height: 14px;
                        border: 1px solid $body-border-color;
                        background: $body-second-color;
                        @include ab(3px, -20px, n, n);
                        @include border-radius(3px, 3px, 3px, 3px); 
                    }
                } 
                input:checked + .wrap-item,
                input:checked + .b-info {
                    &:after {
                        content: '\e946';
                        font-family: $font-family-icon;
                        font-size: 10px;
                        line-height: 12px;
                        color: $body-general-color;
                        @include ab(4px, -18px, n, n);
                    }
                }       
                .bundled-price {
                    display: inline-block;
                    @include padding-start(5px); 
                    .price {
                        font-size: $body-font-size + 2px;
                        color: $body-price-sale-color;
                    } 
                }
                &:not(:first-child) {
                    clear: both;
                    cursor: pointer;
                    .bundled-product-title {
                        color: $body-title-product;
                        border-bottom: 1px solid $body-title-product;
                    }
                    input[type="checkbox"] {
                        cursor: pointer;
                    }
                }
                &:first-child .bundled-product-title {
                    color: $body-light-color;
                } 
            }   
            .wrap-bundled-item {
                @media (max-width: 991px) and (min-width: 576px) {
                    width: calc(100% - 200px);
                    float: left;
                }
            }
        }
    }   
    .col-lg-9 {
        .product-simple-tab {
            padding: 0;
        }    
        .tab-content { 
            @include padding-start(4.05%); 
            @include padding-end(4.05%);
            @media (max-width: 767px) {
                @include padding-start(5%);  
                @include padding-end(5%);
            }       
        }
    }
