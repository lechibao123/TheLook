                @media (max-width: 1199px) {
                    display: block;  
                }
            }                          
            .product-button {
                position: static;
                border: none;
                background: none;
                padding: 0;    
                text-align: start;
                z-index: 1;
                margin: 0 -4px; 
                align-items: center;
                justify-content: space-between;  
                @include opacity(1);  
                @include flexbox; 
                @include translate(0, 0);
                @media (max-width: 767px) {
                    display: none;
                } 
