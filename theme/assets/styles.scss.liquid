            list-style: disc outside none;
            padding: 0 0 20px;
            @include padding-start(20px);
            li {
                margin: 0 0 5px;
                line-height: 24px;
                p {
                    display: inline;
                }
            }
            &:last-child {
                padding-bottom: 0;
            }
        }
    }
    .share-links {
        margin: 40px 0 35px;
        @media (max-width: 767px) {
            margin-top: 20px;
        }
    }  
