            padding: 0;  
            @media (min-width: 768px) {
                @include flexbox();
            }
            &:before,
            &:after {
                content: none;
            }   
            .product-head {  
                position: relative;  
                margin: 0;                   
                padding: 0;
                @include padding-end(15px);        
                @media (min-width: 768px) {       
                    max-width: 75%;
                    @include flex(0 0 75%);
                }        
                @media (max-width: 767px) {
                    width: 100%;    
                    display: block;   
                    padding: 0 
                }                             
            }
            .featured-img {
