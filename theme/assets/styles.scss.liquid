            font-family: $font-heading;
            font-weight: 600;
            margin: 0 0 5px;
            font-size: $body-font-size + 2px;
        }
        .address {
            margin: 0 0 7px;      
        }
        .misc {
            margin: 0 0 7px;
        }
        > a {      
            text-decoration: underline; 
            @include margin-end(10px);
        }
    }
}
