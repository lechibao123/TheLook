                }
            }
        } 
        .btn-add-cart {
            width: auto;
            height: auto;
            justify-content: center;
            @include button1; 
            color: $white;
            line-height: 22px;
            padding: 10px 20px; 
            align-items: center; 
            @include flexbox;
            .text {
                display: block;  
            }
            .demo-icon {
                display: none;
            }
        }
    }
