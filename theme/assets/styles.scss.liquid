    font-weight: $font-bold;
}
.not-available {
    color: $main-color;
}
.pagination-holder {      
    width: 100%;
    display: table;
    margin: 30px 0 0;
    .pagination {
        margin: 0 auto;
        display: block;    
        background: none;
        padding: 25px 0 0;        
        text-align: center;        
        border-top: 1px solid $body-border-color;
        @include border-radius-reset;
        > li {
            margin: 0 1px 5px;
            display: inline-block;      
            > a,
            > span {
                display: block;                
                padding: 0;
                line-height: 33px;
                min-width: 35px;
                height: 35px;
                background: none; 
                margin: 0;        
                font-size: $body-font-size;
                color: $body-general-color; 
                border: 1px solid $body-border-color;
                @include border-radius-circle;                             
                &.disabled {
                    @include opacity(0.7);
                    cursor: no-drop;
                    display: none;
                }
            }
            .next,
            .prev {
                font-size: $body-font-size;  
                width: auto;
                border: none;  
                &:hover {
                    background: none;
                    color: $body-title-color;
                    font-weight: 700;
                }    
            }
            &:hover {       
                > a, > span {                    
                    @media (min-width: 1025px) {
                        color: $main-color-text;
                        border-color: $main-color;
                        background: $main-color;                          
                    }
                }
            }      
            &.active {        
                > a, > span {          
                    color: $main-color-text;
                    border-color: $main-color;
                    background: $main-color;   
                    font-weight: 700;                                   
                }
            }
            &:first-child {   
                float: left; 
                @include margin-start(0); 
            }      
            &:last-child {
                float: right;
            }
        }
    }
}
/*=== Collection page ===*/
.templateListCollections {
    .bottom-container,
    #footer-content {
        margin-top: -1px;
        position: relative;
        z-index: 1;
        background: $body-background-color;
    } 
}
.fix-boxed .category-grid-view {
    @media (min-width: 1440px) {
        max-width: 1200px;
    }     
}
.category-grid-view {
    @include width-boxed;      
    .row {
        margin: 0;
    }
    .collection-item {
        padding: 0;
        @media (min-width: 992px) {
            &.col-lg-2:nth-child(6n + 1) {
                .category:before {
                    content: none;
                }   
            } 
        } 
        @media (max-width: 991px) and (min-width: 768px) {
            &.col-md-3:nth-child(4n + 1) { 
                .category:before {
                    content: none; 
                } 
            }
        }
        @media (max-width: 767px) and (min-width: 576px) {
            &.col-sm-4:nth-child(3n + 1) {
                .category:before {
                    content: none; 
                } 
            }
        }
        @media (max-width: 575px) {
            &.col-6:nth-child(2n + 1) { 
                .category:before {
                    content: none; 
                }  
            }
        }
    }  
}
.category {    
    border-bottom: 1px solid $body-border-color;
    margin: 0;
    padding: 20px 20px 50px;      
    position: relative;
    height: 100%;   
    @include transition(all 0.35s ease 0s);
    @media (max-width: 991px) {
        padding: 15px 15px 30px;   
    }  
    &:before {
        content: '';
        width: 1px;
        height: auto;
        background: $body-border-color;
        @include ab(50px, 0, 50px, n);
        @media (max-width: 575px) {
            top: 30px;
            bottom: 30px; 
        }
    }
    .category-name {                
        margin: 0 auto; 
        font-size: $size-h5;       
        color: $body-title-color; 
        @media (max-width: 991px) {
            font-size: $body-font-size + 1px; 
        }
        @media (max-width: 767px) {
            font-size: $body-font-size - 2px;
            font-weight: $font-bold;
        }      
        &:hover {
            @media (min-width: 1025px) {
                color: $main-color;
            }      
        } 
    }
    .category-image {
        margin: 0 auto 20px;
        > a {
            display: block;
            position: relative;                 
        }
    }
    &:hover {
        @media (min-width: 1025px) {
            @include box-shadow(0 0 5px 0 #c6c6c6);
            z-index: 4;
            &:before {
                content: none;
            }
        }
    }  
}
/*=== product-col-sku ===*/ 
.product-col-sku {
    border-top: 1px dashed $body-border-color;
    border-bottom: 1px dashed $body-border-color;
    text-align: center;  
    margin: 50px 0;
