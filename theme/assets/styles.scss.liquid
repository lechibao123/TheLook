        margin: 0 auto;
        clear: none;    
        .spr-form-title {
            font-size: $size-h5;
            margin: 0 0 35px 0;
            color: $body-title-color;  
            font-weight: 400;    
            @media (max-width: 767px) {
                margin-bottom: 10px;
            }
        } 
        .spr-container {
            padding: 0;
            border: 0;
            .spr-header-title {      
                margin: 0 0 15px;
                font-size: $size-h5;
                text-transform: none;
                text-align: start;     
            }
            .spr-summary {
                text-align: start;
                .spr-starrating {    
                    float: left;    
                    margin: 13px 0 0;
                    @include margin-end(5px);
                    .spr-icon {
                        width: 16px !important;
                        float: left;
                        margin: 0;
                        top: 0;
                        &:before {
                            font-size: 14px !important;              
                            float: left;                    
                        }                      
                    }
                }
                .spr-summary-caption {
                    display: inline-block;
                    margin: 11px 0;        
                    color: $body-general-color;                
                }
            }
            .spr-summary-actions {
                float: right;
                a {
                    display: inline-block;
                    @include button1(); 
                    padding: 5px 30px;
                    color: $white;
                    font-weight: 700;
                }
            }
            .spr-button, .spr-button-primary {
                @include button1();  
                color: $white;      
                font-weight: 700; 
                line-height: 27px;
                padding: 5px 30px; 
            }
            .spr-form-contact, .spr-form-review, .spr-form-actions {
                max-width: 500px;
            }
            .spr-form-input-text, .spr-form-input-email {
                max-width: 315px;
                height: 37px;
                padding: 8px 17px;
                line-height: 18px;
                float: right;
                clear: none;
                @include border-radius(15px, 15px, 15px, 15px);  
                @media (max-width: 767px) {
                    max-width: 300px; 
                }  
                @media (max-width: 575px) {
                    max-width: 100%;
                    float: none;
                }
            }
            .spr-form-label {
                font-weight: 400;
                float: left;
                width: 155px;
                text-align: end;
                line-height: 38px;
                color: $body-title-color;
                font-size: $body-font-size; 
                margin: 0;
                @include margin-end(10px);
                @media (max-width: 767px) {
                    text-align: start;
