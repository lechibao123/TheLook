            padding: 15px 20px;         
            border: none;    
            border-bottom: 1px solid $body-border-color;
            text-align: center; 
            @media (max-width: 767px) {
                border-bottom: none;
                &:before {
                    font-weight: 400;
                    color: $body-light-color;
                    font-size: $body-font-size;
                }
            }
            &.product-title {
                color: $body-title-color;
                font-size: $size-h5 - 2px;  
            }
            &.product-price {
                font-size: 16.43px;
                color: $body-price-color;  
            }
            &.product-available {
                color: #5bb300;
            }
            &.product-quantity input {
                height: 35px; 
                max-width: 140px;
                width: 100%;
                padding: 0 10px;
                text-align: center;
            }
            &.product-variants select {
                line-height: 35px;
                height: 35px; 
            }
            &.product-detail {
                .view-btn {
                    white-space: nowrap; 
                    padding: 4px 25px; 
                    line-height: 27px; 
                    font-weight: 700;
                    margin: 5px;
                    font-size: $body-font-size;     
                    @include border-radius-25;
                    @include transition(all 0.35s ease-in-out 0s);
                    background: $button-btn1;   
                    color: $button-btn1-txt;
                    border: 1px solid $button-btn1-border;    
                    &:hover, &:focus, &:active {
                        background: $button-hover-btn1;  
                        color: $button-hover-btn1-txt;
                        border-color: $button-hover-btn1-border;    
                    } 
                    &:last-child {
                        background: $button-btn2;  
                        color: $button-btn2-txt;
                        border: 1px solid $button-btn2-border;  
                        &:hover, &:focus, &:active {
                            background: $button-hover-btn2;  
                            color: $button-hover-btn2-txt;
                            border-color: $button-hover-btn2-border;    
                        }
                    }
                }
            } 
        }
    }
}
