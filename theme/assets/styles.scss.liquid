            width: 85px;
            height: 2px;
            background: $main-color;
            @include ab(n, 0, -1px, n);
        }
    }
    .account-details {
        position: relative;
        > a[title="Logout"] {
            @include ab(10px, n, n, 0);
        }   
        .btn {
            @media (max-width: 1440px) and (min-width: 768px) { 
                display: block;
                width: 100%;
                margin-bottom: 15px; 
            }
            @media (max-width: 575px) {
                display: block;
                width: 100%;
                margin-bottom: 15px; 
            }
        }  
    }
    h4.title > a {      
        letter-spacing: 0;
        color: $body-general-color;
    } 
}
.account-detail {
    padding: 0 0 20px;
    i {
        color: $main-color;
        font-size: 18px;
        margin: 0;
        @include margin-end(7px);
    }
}
.account-details {
    @media (max-width: 767px) {
        padding-bottom: 30px;
    }
    h5 {
        margin: 0 0 15px;
    }
    .info {
        .author,
        .email {
            margin: 0 0 10px;
        }
        .demo-icon {
            color: $main-color;
            margin: 0;
            @include margin-end(7px);
        }
    }
}
/*=== Page search ===*/
.page-search > h4 {    
    margin-bottom: 30px;    
    text-align: center;
} 
.templateSearch {
    .page-search .search-form {
        margin: 0 auto;
        display: block;
        max-width: 500px;
        position: relative;
        .search_box {
            width: 100%;
        }
        .search-submit {
            @include ab(0, n, n, 10px);    
            @include transition(all 0.35s ease 0s);
            line-height: 45px;
            background: none;
            border: none;
            i {
                font-size: $font-small-icon - 2px; 
            }
            &:hover {
                color: $main-color;
            }
        }
    } 
    .cata-product .article-grid-item {
        letter-spacing: 0;
        vertical-align: top;
        float: none;   
        display: inline-block;       
        @media (min-width: 992px) {
            width: 50%;
            &:nth-child(even) {
                @include padding-start(15px);
            }
            &:nth-child(odd) {
                @include padding-end(15px);
            }
        }
        @media (max-width: 991px) {
            width: 100%;
        }
        .post-to-link .btn {
            padding: 5px 40px;
            line-height: 30px;
            font-weight: 400;
            font-size: $body-font-size + 1px;
        }
    }  
}
/*=== Page 404 ===*/
.template404 #main-content {
    padding: 0;
}
.page-404 {  
    width: 100%;  
    .box-404-text {    
        h1 {      
            font-weight: 300;
            margin: 0 auto 20px;
            font-size: 72px;
        }
        p {
            font-size: $size-h4;
            font-weight: 300;
            margin: 30px 0;      
        }
        .btn {
            @media (max-width: 575px) {
                width: 100%;  
            }
            i {
                @include margin-end(7px);
            } 
        }
    }   
}
/*=== Page FAQS ===*/
.page-faq {
    .page-heading {
        margin-bottom: 65px;
    }  
    .faq-ship-title {
        font-size: $size-h2 - 5px;
        font-weight: 400;
        padding-bottom: 22px;
        margin-bottom: 47px; 
        border-bottom: 1px solid $body-border-color;
    }
    .faq-ship-column h3 {
        margin-bottom: 34px;
        font-size: $size-h3 - 6px;
        font-weight: 700;
    }
    .faq-ship-content {
        padding-bottom: 99px; 
        .faq-ship-row {
            padding-bottom: 50px;
            > div { 
                padding-bottom: 35px;
            }   
        }
        .text-content {
            line-height: 22px;
        }
    }
    .faq-accordion {
        h2 {
            font-size: $size-h2 + 10px;
            font-weight: 400;
            text-align: center; 
            margin-bottom: 50px; 
        }
        .panel-faq {
            border-bottom: 1px solid $body-border-color;
            padding: 25px 0;
            &:last-child {
                border-bottom: none; 
            }
        }
        .panel-title {
            font-weight: 400;
            font-size: $size-h4 + 5px; 
            margin-bottom: 0;
            position: relative;
            a {
                display: inline;
                cursor: pointer; 
                &:after {
                    content: "\f004";
                    font-size: $font-small-icon - 4px;
                    font-family: $font-family-icon;
                    float: right;
                    line-height: 30px;
                    @include transition(transform 0.25s ease-in-out); 
                }  
                &[aria-expanded="true"]:after {
                    @include rotate(180);    
                } 
            }   
        }
        .panel-collapse {
            color: #747474;
            padding-top: 45px;
            line-height: 22px;
        }
    }
}
/*=== Register -Login ===*/
h2.heading,
.semi-bold {  
    line-height: 1.0;
    margin-bottom: 20px;    
}
.semi-bold {
    font-size: $size-h5;
    font-weight: normal;
    line-height: 36px;   
    margin: 0 0 12px;
}
.list-benefits > li {
    color: $body-title-color;   
    margin-bottom: 3px;
    line-height: 26px;
    i {
        color: #5bb300; 
        margin: 0;
        @include margin-end(5px);
    }
}
.control-wrapper {
    display: block;
    clear: both;
    &:last-child {
        padding-top: 10px;
    }
    label {
        display: block;
        clear: both;    
        margin-bottom: 10px;  
        font-weight: 700;
        color: $body-title-color;
        font-size: $body-font-size;        
        .req {
            color: $body-title-color;
            @include padding-start(3px); 
        }
    }
    input , select {
        display: block;
        clear: both;
        margin-bottom: 20px;   
        width: 100%;    
    }
    input[type="text"]:-webkit-autofill,
    input[type="email"]:-webkit-autofill,
    input[type="password"]:-webkit-autofill {
        @include box-shadow(0 0 0 0 white inset !important); 
    }
    textarea {
        max-width: 670px;
    }
    button, .btn-default {
        @include margin-end(8px);  
    }
    .action {
        margin: 0 0 27px;    
    }  
    .forgot-pass {
        float: right;
    }
}
.form-wrapper {
    padding: 0 9.65%;
    @media (max-width: 991px) and (min-width: 768px) {
        padding: 0 7%;
    }
    @media (max-width: 767px) {
        padding: 0 5%;
    }
    p {
        line-height: 28px;
        margin: 0;
    }
}
.login-or,
.register-or {
    position: relative;
    @include border-start(1px solid $body-border-color);
    &:before {
        content: '{{ "general.text.or" | t }}'; 
        width: 50px;
        height: 50px;
        text-align: center;
        line-height: 50px;
        font-style: italic;    
        border: 1px solid $body-border-color;
        background: $white;
        @include ab(50%, 0, n, n);
        @include translate(-50%, -50%);
        @include border-radius-circle;
    }
}
.login-or {
    @media (max-width: 767px) {
        margin: 20px 0 0;
        @include border-start(none);
        @include border-end(none);
        padding-top: 50px;
        &:before {
            left: 50%;
            right: auto;
            top: 0;
            z-index: 2;       
        }
        &:after {
            content: '';
            height: 1px;
            background: $body-border-color;
            z-index: 1;
            @include ab(0, calc(5% + 15px), n, calc(5% + 15px)); 
        }
    } 
}
.register-or {  
    @include border-end(1px solid $body-border-color);
    @include border-start(none);
    &:before {
        left: auto;
        right: -50px;
    }
    @media (max-width: 767px) {
        padding-bottom: 50px;
        margin: 0 0 40px;
        @include border-start(none);
        @include border-end(none);
        &:before {
            left: 50%;
            right: auto;
            top: auto;
            bottom: -25px;
            z-index: 2;             
            @include translate(-50%, 0);
        }
        &:after {
            content: '';
            height: 1px;
            background: $body-border-color;
            z-index: 1;
            @include ab(n, calc(5% + 15px), 0, calc(5% + 15px)); 
        }
    }
}
#customer_login, #create_customer {
    padding: 30px 0;
}
#recover-password form {
    padding: 30px 0;
} 
.page-register, .page-login {
    .col-xs-12 {
        @media (max-width: 767px) {
            max-width: 100%;
            @include flex(0 0 100%);
        }
    }
    .alert-danger {
        display: none;
    }
    .login-error-active .alert-danger {
        display: block;
    }
}
.page-register, .page-login , #register-form, #recover-password {    
    .heading {
        font-size: $size-h2 - 5px;
        font-weight: 400;   
        margin: 0 0 10px;   
        border-bottom: 1px solid $body-border-color;
        @include title-wrapper;
        &:before {
            width: 115px;
        } 
    } 
    .btn {            
        margin-bottom: 10px; 
        min-width: 130px;
        @media (max-width: 575px) {
            display: block;
            width: 100%;
        }
    }      
}
#customer-login .text {
    margin: 0 0 30px;
    display: block;
}
#customer_orders h5 {
    margin: 0 0 20px;
}  