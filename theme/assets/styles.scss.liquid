/*toolbar*/
.cata-toolbar {
    margin: 0 0 20px;
    padding: 7px 20px; 
    width: 100%;    
    background: $body-second-color;
    @include border-radius(9px, 9px, 9px, 9px);
    @media (max-width: 767px) {
        @include padding-start(10px); 
        @include padding-end(10px); 
    }
    .group-toolbar {
        align-items: center;  
        @include flexbox;
        @media (min-width: 768px) {
            justify-content: space-between;
        }   
    }
    .filter-icon-order {
        float: left;
        line-height: 38px;
        cursor: pointer;  
        padding: 0;
        @include padding-end(4%); 
        i {
            float: left;
            font-size: $font-small-icon;
            color: $body-light-color;
            @include margin-end(7px); 
            @include transition(all 0.35s ease 0s);
            &:before {
                display: block;
                line-height: 38px;
            }
        }
        &:hover i {
            color: $body-title-color;
        } 
    }  
    .grid-list {    
        .text {
            display: none;          
        }   
        .grid {
            margin: 0 5px;     
            float: left;
            text-align: center;
            border: none;     
            cursor: pointer; 
            font-size: 0;  
            background: none;
            color: $body-light-color;      
            @include transition(all 0.35s ease 0s);      
            &.grid-3,
            &.grid-2 {
                @media (max-width: 767px) {
                    display: none; 
                }  
            }
            svg {
                width: 18px;
                height: 18px;
            }
            &:first-child {
                @include margin-start(0); 
            }
            &.active, &:hover {
                background: none;
                color: $black;        
            }
        }    
    }
    .pagination-showing {   
        @media (max-width: 767px) {
            display: none;
        }
    }   
    .sort-by {
        color: $body-general-color;
        margin: 0 2% 0;  
        @media (max-width: 767px) {
            @include margin-start(auto);
        }
        .sort-by-inner {
            float: left;     
            position: relative;       
            padding: 0 25px;
            background: $white;      
            border: 1px solid $body-border-color;
            @include border-radius-25;
            @media (max-width: 991px) {
                float: none;
                display: inline-block;        
            }         
            @media (max-width: 479px) {
                @include padding-start(5px); 
                @include padding-end(5px); 
            }
            label {
                float: left;         
                line-height: 36px;
                margin: 0;          
                color: $body-title-color; 
            }
            #cata_sort_by {
                float: left;
                position: static;
                &:after {
                    content: none;
                }
                button {
                    float: left;
                    border: none;
                    height: 36px;
                    padding: 0;
                    background: none;
                    line-height: 36px;
                    text-align: start;   
                    font-size: $body-font-size;       
                    a {
                        color: $body-title-color;   
                    }
                    span {
                        padding: 0 10px;
                        float: left;   
                        color: $body-title-color;                  
                        @media (max-width: 479px) {
                            @include padding-end(6px); 
                        }
                    }
                    i {
                        float: right;            
                        text-align: end;
                        padding: 0;
                        content: none;      
                        line-height: 38px;  
                        color: $body-title-color;                
                    }
                }
            }         
        }
        #sort_by_box {      
            width: auto;
            min-width: 175px;      
            background: $white;           
            padding: 0;      
            list-style: none;
            border: 1px solid $body-border-color;
            @include dropdown-effect;
            @include ab(100%, -1px, n, -1px);
            @include translate(0, 10px);         
            @include box-shadow(0 6px 12px rgba($black, 0.176));  
            @media (max-width: 767px) {
                left: auto;
                right: 0;  
            }
            &:before {
                content: '';
                height: 2px;
                background: $main-color;
                @include ab(-1px, -1px, n, -1px);      
            }  
            li {
                padding: 0 10px;
                text-align: start;
                a {
                    padding: 4px 0;    
                    line-height: 20px;      
                    display: block;
                    transition: none;                               
                    font-size: $body-font-size - 1px;
                    color: $body-general-color;
                }
                &:hover { 
                    @media (min-width: 1025px) {
                        background: $body-second-color;
                        a {
                            color: $body-title-color;            
                        }
                    }          
                }
                &.active {          
                    background: $body-second-color;
                    a {
                        cursor: pointer;
                        color: $body-title-color;            
                    }
                }        
            }
        }
        &.opened #sort_by_box {
            @include dropdown-effect-hover; 
        } 
    }
    .show-by {
        display: inline-block;
        position: relative;
        margin: 0 0 10px;
        @include margin-start(4%); 
        .show-by-button {
            border: none;
            padding: 6px 20px;
            line-height: 38px;
            background: $body-second-color;  
        }
        #show-by-box {
            width: auto;     
            min-width: 85px;
            background: $white;          
            padding: 10px 20px;
            display: block;  
            visibility: hidden;
            z-index: -99;
            list-style: none;
            text-align: start;
            @include ab(100%, 0, n, 0);
            @include opacity(0); 
            @include translate(0, 10px);  
            @include  transition(all 0.35s ease);
            li {
                padding: 0;        
                margin-bottom: 0; 
                border-bottom: 1px solid $body-border-color;
                a {
                    padding: 8px 0;
                    line-height: 26px;
                    display: block;
                    transition: none;                               
                    font-size: $body-font-size;
                    color: $body-general-color;
                }
                &:hover {     
                    @media (min-width: 1025px) {
                        a {
                            color: $main-color;            
                        }
                    }         
                }
                &.active { 
                    > span {
                        padding: 8px 0;
                        line-height: 26px;
                        display: block;           
                        font-size: $body-font-size;
                        color: $main-color; 
                    }         
                    a {
                        cursor: default;
                        color: $main-color;           
                    }
                }
                &:last-child {
                    border-bottom: none;          
                }
            }
        }
        &:hover,
        &.opened {
            #show-by-box {
                visibility: visible;
                z-index: 99;          
                @include opacity(1);  
                @include translate(0, 0);
            }
        }
    }
    .top-pagination-holder {
        float: right;
        margin: 5px 0;
        > span {
            float: left;
            line-height: 40px;
            padding: 0;
            @include padding-end(7px); 
        }  
        .pagination {
            float: left;
        } 
    }
}
.mobile-filter {
    text-align: center;  
    color: $body-title-color;
    background: $body-second-color;
    margin: 0 auto;  
    float: right;
    line-height: 28px;
    padding: 8px 25px;
    @include border-radius-50;
    @include transition(all 0.35s ease 0s);
    .fa {
        @include margin-end(8px); 
    }
    &:hover {
        background: $main-color;
        color: $main-color-text;
    }
}
.pagination {
    margin: 0;
    @include border-radius-reset;   
    > li {
        > a,
        > span {
            border: none;
            padding: 0 6px;
            color: $body-general-color;
            background: none;
            line-height: 40px;  
            .fa {
                font-size: 13px;
            }
            &:focus,
            &:hover {  
                background: none;
                color: $main-color;
            }
        }
        &.disabled {
            display: none;
        }
        &.active {
            > a,
            > span {
                background: none;
                color: $main-color;
                &:focus,
                &:hover {  
                    background: none;
                    color: $main-color;
                }
            }
        }
        &:focus,
        &:hover {      
            > a,
            > span {
                background: none;
                color: $main-color;
            }      
        }
    } 
}
.cata-product {
    margin: 0;  
    padding: 0;
    vertical-align: top;
    clear: both;  
    flex-wrap: wrap; 
    @include flexbox;   
    .product-grid-item {     
        padding: 0;
        margin: 0;   
        letter-spacing: 0;
        vertical-align: top;    
