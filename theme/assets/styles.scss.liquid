            }
        }
        .panel-collapse {
            > .panel-body, > .list-group {
                border: none;
                border-bottom: 1px solid $body-border-color;
                padding: 25px 0;
                background: none;
            }
        }
        + .panel {
            margin-top: 0;
        }
        .panel-body {   
            font-size: $body-font-size;
            line-height: 24px;      
            iframe {
                max-width: 100%;
            }
            ul li {
                padding: 1px 0;
            } 
        }
        &:last-child {
            padding-bottom: 0;
            border-bottom: none;
            .panel-heading {
                border-bottom: none;
            }
            .panel-collapse {
                > .panel-body, > .list-group {
                    border-top: 1px solid $body-border-color;
                    border-bottom: none;
                }
            }      
        }
    }
}
.spr-icon { 
    @include opacity(1 !important);
    background: none !important;
    display: inline-block;
    width: 16px !important;
    height: 14px !important;
    font-style: normal;  
    &:before {   
        content: "\e830" !important;
        font-family: $font-family-icon !important;
        font-size: 14px !important;
    }
    &.spr-icon-star-empty {
        color: $body-ratting-empty-color;
        &:before {   
            color: $body-ratting-empty-color;
            content: "\e831" !important;
        }
    }
}
#main-column {
    margin: 35px auto 0;
}
.main-content{ 
    position: relative; 
    clear: both; 
    > section.cata-header {  
        position: relative;
        .catalog-title {     
            width: 100%;
            text-align: center;
            z-index: 1;
            margin: 0;
            font-size: 48px;
            @include ab(25%, 0, n, n);
        }
        > img {
            min-height: 104px;
        }
    } 
}
/*=== breadcrumb ===*/
.breadcrumb-title {
    display: block;
    font-size: $size-h1;  
    color: $body-title-color;       
}
.breadcrumb {
    padding: 0;
    margin: 10px 0 30px;
    background: none;           
    display: flex;
    align-items: center;
    flex-wrap: nowrap;
    white-space: nowrap;
    height: 100%; 
    overflow: hidden;
    overflow-x: auto;   
    @include border-radius-reset;  
    &::-webkit-scrollbar {   
        height: 6px; 
        background: #eee;
        @include border-radius-25;
    } 
    &::-webkit-scrollbar-thumb {
        background: $main-color;
        @include border-radius-25;
    }
    > li { 
        position: relative;   
        line-height: 36px;
        color: $body-general-color;
        padding: 0 20px;     
        &:first-child {
            @include padding-start(0);
        }
        &:last-child { 
            @include padding-end(0);
        }
        + li {
            &:before {
                @include next-icon;      
                font-family: $font-family-icon; 
                margin: 0;
                font-size: 12px;
                color: $body-general-color;            
                @include ab(1px, 0, n, n);      
                font-weight: normal;   
            } 
            > a {  
                background: $body-second-color;
                color: $body-title-color; 
                padding: 0 13px;
                @include border-radius(5px, 5px, 5px, 5px);
            }       
        }
        > a {           
            display: inline-block;      
            padding: 0;
            line-height: 36px;
            position: relative;    
            color: $body-general-color;
            &:hover {   
                color: $body-title-color;         
            }
        } 
        &.active { 
            color: $body-title-color;  
            font-weight: 500;  
        }
    }  
}  
.breadcrumb-page-title {
    display: block;
    width: 100%;    
    color: $body-title-color;
    font-size: $size-h2;
    font-family: $font-heading;      
    margin: 0 auto 0;  
}
.bw-image {  
    clear: both;
    width: 100%; 
    height: $breadcrumb-height;  
    margin: 0 0 50px;
    overflow: hidden;    
    position: relative;
    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;    
    @media (max-width: 479px) {   
        margin-bottom: 30px;
    }
    .breadcrumb-holder {    
        width: 100%;
        @media (min-width: 768px) {
            display: table;
            vertical-align: middle;
            height: 100%;
        }
        @media (max-width: 767px) {
            padding: 0 15px; 
            @include ab(50%, 50%, n, n);
            @include translate(-50%, -50%);
        }
    }    
    .breadcrumb {
        margin: 0;     
    }
}
.bw-color {
    background: $breadcrumb-bg-color;
    margin: 25px 0 23px;      
    overflow: hidden; 
    @media (max-width: 767px) {
        margin-top: 8px;
        margin-bottom: 8px;
    } 
    .breadcrumb-holder .breadcrumb {
        margin: 0;  
    } 
}
.breadcrumb-product-page {
    background: $body-second-color; 
    margin: 0 auto 45px;  
    .breadcrumb-holder {
        padding: 18px 5%;
        margin: 0 auto;
        @media (min-width: 1400px) {
            @include padding-start(75px); 
            @include padding-end(75px); 
        }
        @media (max-width: 767px) {
            @include padding-end(80px); 
        }   
    }  
    .container {
        .breadcrumb-holder {
            @include padding-start(0); 
            @include padding-end(70px); 
        }
        .product-control-buttons {
            .next {
                right: 0;
            }
            .prev {
                right: 35px;
            }
        }
    }
    .breadcrumb {
        text-align: start;     
    }
}
.breadcrumb-none #body-content {
    padding-top: 30px;
} 
/*=== comments ===*/
.comment-form {
    h3 {      
        font-weight: 400;     
        border-bottom: 1px solid $body-border-color;    
        margin: 0 0 40px;   
        font-size: $size-h3 + 3px;
        @include title-wrapper;
        @media (max-width: 767px) {
            margin-bottom: 25px;  
        }
        &:before {
            width: 280px;   
        }
    }
    p {
        margin: 0 0 30px;
    }   
    .control-wrapper {
        input {
            width: 100%;             
            margin-bottom: 25px;  
            height: 38px;
            padding: 8px 17px;    
        }
        textarea {
            margin: 0;
            width: 100%; 
            max-width: 100%;      
            min-height: 180px;
            padding: 8px 17px;  
        }    
        &:last-child {
            padding-top: 0;
        }    
    }
    > .btn {
        margin: 25px 0 0;
        padding: 5px 30px;
        @media (max-width: 575px) {
            display: block;
            width: 100%;
        }
    }
}
.grid-comments {
    h5 {
        font-weight: 400;     
        border-bottom: 1px solid $body-border-color;    
        line-height: 32px;
        padding-bottom: 13px;     
        margin: 0 0 40px;    
        position: relative;
        font-size: $size-h3 + 3px;
        @media (max-width: 767px) {
            margin-bottom: 25px;
        }
        &:before {
            content: '';
            display: block;
            width: 300px;
            height: 2px;
            background: $main-color;
            max-width: 100%;
            @include ab(n, 0, -1px, 0);
        }
    }
    .comments {
        list-style: none;
        margin: 45px 0;
        @media (max-width: 767px) {
            margin: 25px 0;
        }
        .comment {
            border-bottom: 1px solid $body-border-color;
            margin: 0 0 35px;
            padding: 0 0 35px;
            @media (max-width: 767px) {
                margin-bottom: 25px;
                padding-bottom: 25px;
            }
            &:last-child {
                padding-bottom: 0;
                margin-bottom: 0;
                border-bottom: none;
            }
        }
        .load-comment {
            display: table;
            width: 100%;
            .comment-avatar {
                display: table-cell;
                vertical-align: top;
                width: 100px;
                @include border-radius-circle;
                @media (max-width: 479px) {
                    width: 80px;
                }
                img {
                    width: 100%;
                    @include border-radius-circle;
                }
            } 
            .comment-text {
                display: table-cell;
                vertical-align: top;
                padding: 0;
                @include padding-start(20px); 
                .author {
                    font-weight: 700;
                }
                .date {
                    font-size: $body-font-size - 1px;
                }
                p {
                    margin: 25px 0 0;
                }
            }
        }
    }
}
/*=== popup-sell-people ===*/
.popup-sell-people {
    width: 350px;
    display: table;
    vertical-align: middle;
    position: fixed;
    z-index: 9999;
    display: none;
    background: #ffffff;  
    @include border-radius(5px, 5px, 5px, 5px);
    @include box-shadow(0 0 5px 0 #c6c6c6); 
    @media (max-width: 370px) {
        width: calc(100% - 20px);    	
    }
    &.show {
        display: block;
    }
    &.bottom-left {
        bottom: 10px;
        left: 10px;
        @media (max-width: 991px) {
            bottom: 64px;
        } 
    }
    &.bottom-right {
        bottom: 10px;
        right: 10px;
        @media (max-width: 991px) {
            bottom: 64px;
        } 
    }
    &.top-left {
        top: 10px;
        left: 10px; 
    }
    &.top-right {
        top: 10px;
        right: 10px;
    }
    .img-user {
        width: 80px;		
        display: table-cell;
        vertical-align: middle;
        img {
            width: 100%;
            @include border-radius(5px, 0, 0, 5px);
        }
    }	
    .text-detail {
        display: table-cell;
        vertical-align: middle;
        padding: 10px 15px;
        .sell-text {
            margin: 0;
            line-height: 20px;
            font-size: $body-font-size;
            font-weight: normal;
        }
    }
}
/*=== discount-modal ===*/
#discount-modal {
    display: none;
    z-index: 99991; 
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    margin: 0 auto;
    .underlay {
        background: rgba(0, 0, 0, 0.5);   
        height: 100%;
        left: 0;    
        position: fixed;
        top: 0;
        width: 100%;
        z-index: 99992;    
    }
    .wrap-modal {
        background: $body-background-color;
        z-index: 99999;
        padding: 0;        
        margin: 0 calc((100% - 700px) / 2);
        width: 700px;
        @include ab(50%, 0, n, n);
        height: auto; 
        max-height: 90%; 
        flex-direction: column; 
        @include flexbox;  
        @include translate(0, -50%);
        @include border-radius-20; 
        @media (max-width: 767px) {      
            width: 90%;
            margin: 0 5%;      
        }
        > a {
            @include ab(-25px, n, n, -20px); 
            @include width-btn-1; 
            z-index: 999991;   
            @include border-radius-circle;    
            @include box-shadow(0 0 0 rgba($black, 0.5)); 
            @include transition(all 0.35s ease 0s);  
            i:before {
                @include close-btn;   
                color: $white;                             
            } 
            &:hover {
                background: $button-hover-btn1;
                i:before {
                    color: $button-hover-btn1-txt;
                } 
            }
        }
    }
    .modal-body {
        padding: 0;  
        width: 100%;  
        height: 100%;
        overflow-y: auto;
        align-items: center;   
        @include flexbox;
        @media (max-width: 767px) {
            display: block;
        }
        > div {
            width: 50%;      
            @media (max-width: 767px) {
                width: 100%;
            }
        }
        .image {
            @include padding-end(15px); 
            @media (max-width: 767px) {
                padding: 0 0 20px;
                text-align: center;
            }
            img {
                @include border-radius(20px, 20px, 0, 0); 
                @media (min-width: 768px) {
                    @include border-radius(20px, 0, 0, 20px);  
                }
            }
        }
        .content {
            padding: 15px; 
            text-align: center;
            max-width: 300px;
            margin: 0 auto;
            @media (max-width: 767px) {
                padding-bottom: 25px;
            }
            h5 {
                text-transform: uppercase;
                font-weight: 700;
                margin: 0 auto;
            }
            p {
                line-height: 18px;
                margin: 5px 0 13px;
            }
            .btn {
                text-transform: uppercase;
                font-size: $body-font-size - 1px;
                padding: 7px 25px;
                line-height: 28px;        
            }
        }
    }
} 

/*=== pre-order-popup ===*/
#pre-order-popup {   
    max-height: 80vh;
    max-width: 680px;
    overflow: visible;
    background: none; 
    @media (max-width: 767px) {
        max-width: 90%;
    }   
    #pre-order-form {
        padding: 40px;
        background: $body-background-color;
        overflow-x: auto;
        overflow-y: auto;
        @include border-radius-25;
        @media (max-width: 479px) {
            padding: 20px;
        }
        .form-control {
            height: 47px;  
            background-repeat: no-repeat;
            background-attachment: fixed;
            font-size: $body-font-size;
            background: $white;
            padding: 13px 30px;   
            line-height: 1.6;    
            color: $body-light-color;
            border: 1px solid $body-border-color;
            -moz-appearance: textfield;  
            -webkit-appearance: textfield;     
            -webkit-appearance: none;   
            @include border-radius-25; 
            @include box-shadow(0 0 0 0 white inset);
            @media (max-width: 575px) {
                @include padding-end(15px);
                @include padding-start(15px);  
            }
            &:focus {
                border-color: $main-color;
                @include box-shadow(0 0 0 1000px white inset);
            }
            @include placeholder {
                color: $body-light-color;
            }
        }
        .form-control:-webkit-autofill {
            @include  box-shadow(0 0 0 1000px white inset);
        }
        textarea {
            min-height: 80px;
        }
    }  
}
/* Category page --------------------------*/
.srp-filter-wrapper {
    display: table;
    width: 100%;
    margin-bottom: 15px;
    > a {
        line-height: 18px;
        display: inline-block;
        font-size: $body-font-size;    
        @include margin-end(15px); 
        @include padding-end(15px); 
        @include border-end(1px solid $body-border-color);   
        &:before {
            content: "\e8cc";
            font-family: $font-family-icon;
            font-size: 12px;
            @include margin-end(7px);          
        }
        &:hover {
            &:before {
                content: '\e8cc'; 
            }
        }
    }
    .srp-filter {
        display: inline-block;
        li {
            @include margin-end(50px); 
            span {
                display: none;        
            }
            a {
                line-height: 18px;
                font-size: $body-font-size;
                &:before {
                    content: "\e8cc";
                    font-family: $font-family-icon;
                    font-size: 12px;
                    @include margin-end(7px); 
                }
                &:hover {
                    &:before {
                        content: '\e8cc';       
                    }
                }
            }
        }
    }
}
.sb-filter-wrapper {    
    .sbw-filter {   
        overflow: hidden; 
        .sbf-title {
            .clear-filter {
                text-transform: capitalize;
                cursor: pointer;
                font-weight: 400;
                @include margin-start(auto);
            }     
            span {                
                cursor: pointer;        
                i {
                    display: none;
                    &:before {
                        content: none;
                    }         
                } 
            }
        }    
    } 
    .sb-filter-tag {
        overflow: auto;
        li {
            margin: 0 0 13px !important;
            line-height: 20px;
            a {
                position: relative;
                min-height: 20px;
                display: inline-block;   
                font-size: $body-font-size;   
                word-break: break-word; 
                color: $body-general-color; 
                @include padding-start(25px);   
                &:before {
                    content: "";
                    @include ab(3px, 0, n, n);          
                    width: 16px;
                    height: 16px;              
                    border: 1px solid $body-border-color;  
                    @include border-radius(3px, 3px, 3px, 3px);
                    @include transition(all 0.35s ease 0s);            
                }  
                &:hover {
                    @media (min-width: 1025px) {
                        color: $body-title-color !important;  
                        &:before {
                            background: $main-color;
                            border: 2px solid $main-color;
                        }                
                    }               
                }
            }
            &.active > span {
                position: relative;
                min-height: 20px;
                display: inline-block;          
                color: $body-title-color;
                font-size: $body-font-size; 
                @include padding-start(25px);  
                &:before {
                    content: "";
                    @include ab(3px, 0, n, n);            
                    width: 16px;
                    height: 16px;              
                    border: 2px solid $main-color;  
                    background: $main-color; 
                    @include border-radius(3px, 3px, 3px, 3px);
                    @include transition(all 0.35s ease 0s)            
                }   
            } 
        }
    }
}
.sbw-filter {  
    > ul,
    > o {
        list-style: none;
        margin: 0;
        padding: 15px;
        li {
            margin: 8px 0;
            a:hover {
                color: #888;
            }
        }
    }
    .price {
        color: $main-color;
    }
    .price-compare {
        color: #aaa;
    }
    .sb-title {
        padding: 14px 15px;
        margin: 0 0 25px;    
    }
    > div {    
        .row {
            margin-bottom: 15px;
            &:last-child {
                margin-bottom: 0;
            }
        }        
        .sbf-title {
            color: $body-title-color;
            line-height: 1.3;
            margin: 0;     
            padding: 0;       
            font-size: $body-font-size;  
            font-weight: 700; 
            align-items: center;   
            @include flexbox;          
        } 
        .sb-filter.size .advanced-filters > li.advanced-filter {
            display: inline-block;
            width: auto;
            float: left; 
            margin: 0 0 10px;
            @include margin-end(10px);   
            a {
                display: inline-block;
                padding: 0 3px;
                min-width: 30px;
                line-height: 26px;
                text-transform: uppercase;
                text-align: center;
                font-weight: $font-bold;
                font-size: $body-font-size;
                border: 2px solid $body-border-color; 
                &:before,
                &:after {
                    content: none;
                } 
                &:hover {
                    color: $main-color;
                    border: 2px solid $main-color; 
                }               
            }
        }
        .advanced-filters {
            list-style: none;
            margin: 25px 0 0;      
            padding: 0;
            max-height: 290px;
            overflow: auto; 
            > li {
                > a {
                    position: relative;
                    @include padding-start(23px);          
                    &:before {
                        content: '';
                        border: 1px solid $body-border-color;
                        width: 15px;
                        height: 15px;
                        background: transparent; 
                        @include ab(3px, 0, n, n);
                        @include border-radius(3px, 3px, 3px, 3px);
                        @include transition(all 0.35s ease 0s);
                    }     
                    &:hover {
                        color: $main-color !important; 
                        &:before {
                            border-color: $main-color !important;
                        }           
                    }
                }
                &.active-filter > a { 
                    color: $main-color !important; 
                    &:before {
                        border-color: $main-color; 
                        background: $main-color;        
                    }         
                }  
                &.advanced-filter {
                    line-height: 20px;
                    margin-bottom: 13px;
                    a {
                        position: relative;
                        min-height: 20px;
                        display: inline-block;    
                        font-size: $body-font-size; 
                        font-weight: 400;  
                        color: $body-general-color; 
                        @include padding-start(25px);              
                        &:before {
                            content: "";
                            @include ab(3px, 0, n, n);              
                            width: 16px;
                            height: 16px;              
                            border: 1px solid $body-border-color;  
                            @include border-radius(3px, 3px, 3px, 3px);
                            @include transition(all 0.35s ease 0s);            
                        }
                        &:hover {
                            @media (min-width: 1025px) {               
                                &:before {
                                    border-color: $main-color;
                                    background: $main-color;
                                }                 
                            }              
                        } 
                    }
                    &.af-color {
                        border: none;
                        padding: 0;
                        float: none;    
                        width: 30px;
                        height: 30px;
                        line-height: 30px;            
                        position: relative; 
                        cursor: pointer;
                        float: left;     
                        display: block;       
                        border: 1px solid $body-border-color;  
                        margin: 0 0 10px;
                        @include margin-end(10px);            
                        @include border-radius-circle;
                        @include box-shadow(0 0 0 0 $body-border-color inset);  
                        @include transition(all 0.35s ease 0s);
                        a {                            
                            padding: 0;
                            position: relative;
                            display: block;  
                            font-size: 0;
                            width: 100%;
                            height: 100%; 
                            border: 1px solid $body-border-color;
                            @include border-radius-circle; 
                            &:before { 
                                content: none; 
                            } 
                        }
                        &:hover {
                            @media (min-width: 1025px) {                
                                @include box-shadow(0 0 0 0 $body-border-color inset); 
                                a {
                                    border: 2px solid $main-color;  
                                }                  
                            }
                        }
                        &.active-filter {              
                            @include box-shadow(0 0 0 0 $body-border-color inset); 
                            a {
                                border: 2px solid $main-color;   
                            }    
                        } 
                    }
                    &.active-filter a {
                        &:before {
                            border-color: $main-color;
                        }
                        &:after {
                            width: 7px;
                            height: 7px;
                            @include opacity(1);
                        }
                    }
                }
            }
        }    
    }  
}
.filter-order-form {
    @include drawer-sidebar;
}
.filter-sidebar { 
    &.position-sidebar {
        &.style-normal .sb-filter-wrapper .f-close {
            @media (min-width: 992px) {
                display: none;
            }
        }
        &.style-accordion .sb-filter-wrapper {
            .accordion {
                .sbf-title {
                    position: relative;
                    cursor: pointer;
                    padding: 3px 0; 
                    @include padding-end(20px);            
                    &:before {
                        content: '\f106';
                        font-family: $font-family-icon;
                        font-size: 14px;
                        width: 20px;
                        height: 20px;
                        line-height: 20px;
                        text-align: center;
                        @include ab(2px, n, n, 0);
                        @include transition(all 0.35s ease 0s);
                    }            
                }
                &.accordion-active .sbf-title:before {
                    @include rotate(180);
                } 
            }        
            .f-close {
                @media (min-width: 992px) {
                    display: none;
                }
            }
        }
    }
    &.position-body {
        &.style-toggle {
            position: relative;
            width: 100%;
            display: none; 
            padding: 25px 15px 0;
            margin: 0 auto 20px;
            background: $body-background-color;
            @include box-shadow(0 0 10px rgba($black, 0.05));
            .sb-title, .f-close {
                display: none;
            }
            .sb-filter-wrapper {
                width: 100%;
                display: table;
                vertical-align: top;
            }
            .sbw-filter {
                width: 25%;
                display: inline-block;
                vertical-align: top;
                margin: 0;
                padding: 0;
                border-bottom: none;
                @media (max-width: 991px) {
                    width: 33.33333%;
                }
                @media (max-width: 767px) {
                    width: 50%;
                }
                @media (max-width: 575px) {
                    width: 100%;
                }
                > div .sb-filter {
                    padding: 0 15px;
                    clear: none;
                    float: none;
                    display: inline-block;
                    letter-spacing: 0;
                    vertical-align: top;
                    border: none;
                    margin: 0 0 30px;
                    width: 100%;
                }
            } 
            .sb-filter-tag {
                padding-bottom: 10px; 
                vertical-align: top;
                li {
                    display: inline-block;
                    letter-spacing: 0;
                    vertical-align: top;
                    padding: 0;
                    @include padding-end(10px);  
                    @media (min-width: 1200px) {
                        width: 16.666667%;
                    }
                    @media (max-width: 1199px) and (min-width: 992px) {
                        width: 20%;
                    }
                    @media (max-width: 991px) and (min-width: 768px) {
                        width: 25%;
                    }
                    @media (max-width: 767px) and (min-width: 556px) {
                        width: 33.333333%;
                    }
                    @media (max-width: 555px) {
                        width: 50%;
                    }
                    &:first-child {
                        display: block;
                        width: 100%;
                    }
                }
            }
        }
        &.style-drawer {
            @include drawer-sidebar;
            h5 {
                font-size: $size-h4;      
                font-weight: 400;     
                text-transform: capitalize; 
                border-bottom: 1px solid $body-border-color;
                line-height: 1.4;
                margin: 0 0 35px;
                padding: 0 0 10px;
                position: relative;
                &:before {
                    content: '';
                    width: 70px;
                    height: 2px;
                    background: $main-color;
                    @include ab(n, 0, -1px, n);
                }
            }
            .sbw-filter > div .sb-filter {
                margin: 0 0 30px; 
                &:last-child {
                    border-bottom: none;
                    padding-bottom: 0;
                }   
            }
        }
    }
}
.group-toolbar {
