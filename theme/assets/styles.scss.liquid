            border-top: 1px solid $body-border-color;
            &:nth-child(-n+2) {
                border-top: none;
            } 
        }           
    }  
    .product-wrapper .product-content .product-description {
        display: block;
    } 
    &.no-padding {
        margin: 0 auto;
        .product-grid-item {
            padding: 0;
        }
    }  
}
.cata-grid-4 {
    .cata-product .product-grid-item {
        @media (min-width: 992px) {
            width: 25%;
            &:nth-child(4n+1) {
                .product-wrapper:before {
                    content: none;
                }
