        margin: 0 auto;
        @include padding-start(15px);   
        @include padding-end(15px);       
    }  
}
.blog-filter {
    margin-bottom: 20px;
}
.page-article .col-xl-8 + .col-xl-4 {
    @media (max-width: 991px) {
        padding-top: 40px;
    }
} 
.post-item .post-item-inner .post-info ul li, 
.post .post-content .post-info ul li {
    display: inline-block;
    position: relative;
    padding: 0;   
    margin: 0 10px; 
    &:before {
        content: '';
        width: 5px;
        height: 5px;          
        background: $body-general-color;
        @include ab(50%, -14px, n, n);
        @include border-radius-circle;
        @include transform(translate(0, -50%)); 
    }
    &:first-child {   
        @include margin-start(0);      
        &:before {
            content: none;
        } 
    } 
    a {
        color: $body-link-color;
        &:hover {
            color: $main-color;
        } 
    }
}
.post {
    .post-image {
        margin: 0 auto 30px;
        text-align: center;
        img {
            width: 100%;
        }
    }
    iframe {
        max-width: 100%;
    }     
    .buttons {
        padding: 0 0 65px;
    }
    .btn-previous,
    .btn-next {    
        font-weight: $font-bold;    
        white-space: normal;
        padding: 5px 0;       
        text-transform: none;
        font-size: $size-h6 + 2px;
    }  
    .btn-previous {
        @include margin-end(10px);
        &:before {
            content: '\e9e9';
            font-family: $font-family-icon;
            font-size: $body-font-size + 2px;
            float: left;
            @include margin-end(20px);     
        }
    }  
    .btn-next {
        float: right;
        &:after {
            content: '\e9ec';
            font-family: $font-family-icon;
            font-size: $body-font-size + 2px;
            float: right;
            @include margin-start(20px);
        }
    } 
    .post-content {
        .post-title {
            font-weight: 400;
        }
        .post-info {
            margin: 0 0 25px;
            padding: 12px 0 18px;  
            border-bottom: 1px solid $body-border-color;
            ul {
                font-size: $body-font-size - 1px;    
                padding: 0;      
            }
        }
        .post-des p {
            margin: 0 0 40px;
