					text-align: center;  
					@include transition(transform 0.35s ease-in-out); 
					&:before {
						content: "\f004";
						font-size: $nav-font-size;
						font-family: $font-family-icon;
						display: block;
						width: 100%;
						height: 100%; 
						color: rgba(255,255,255,.7); 
					}   
				}  
				a {
					font-size: $nav-font-size; 
					position: relative; 
					color: rgba(255,255,255,.7);
				}  
				.dropdown-inner {
					padding: 0 13px;  
					border-bottom: 1px solid #262626;
					align-items: center;
					justify-content: space-between;
					@include flexbox; 
					a {
						padding: 10px 0;  
					} 
				} 
				.dropdown-menu {
					position: static;
					@include opacity(inherit);
					visibility: inherit;  
					border: none;
					background: #090b0c;
					z-index: 1;
					width: 100%;
					padding: 0; 
					display: none;  
					@include box-shadow(none); 
					@include border-radius-reset;  
					@include translate(0, 0); 
					&:before {
						content: none;
					} 
					.dropdown-inner {	
						background: #090b0c; 
						a {
							font-weight: 600;
							font-size: $nav-sub-font-size;  
							color: #ffffff;  
						}   
						span.expand:before {
							content: none; 
						}
					}    
					li {
						a {
							color: rgba(255,255,255,.7);
						}
						&:hover { 
							background: #b3b3b3;
							@include box-shadow(inset -2px 0 4px -1px rgba(0,0,0,.6));
							> a { 
								color: #000;
								text-decoration: none; 
							}   
						} 
					}     
				}   
				&.mega-is-hover { 
					> .dropdown-inner span.expand { 
						@include transform(rotate(180deg));
					}    
					.dropdown-menu {
						display: block;   
					}  
				}
				&:hover {
					&:not(.dropdown) {
						background: #b3b3b3;
						@include box-shadow(inset -2px 0 4px -1px rgba(0,0,0,.6));
					} 
					> a { 
						color: rgba(255,255,255,.7); 
					} 
				}
			} 
		}
	} 
