	margin: 0; 
	border: 1px solid $body-border-color;
	background: $nav-dropdown-background;
	visibility: hidden;
	@include opacity(0);
	@include border-radius-reset;
	@include box-shadow(0 6px 12px rgba($black, 0.176)); 
	@media (max-width: 1199px) and (min-width: 992px) {
		width: 235px;  
	}  
	&:before {
		content: '';
		height: 2px;
		background: $main-color;
		@include ab(-1px, -1px, n, -1px);
	}
	img {
		visibility: inherit;  
	}
	picture {
		@include opacity(0);
		visibility: hidden;
	} 
	> li > a {
		padding: 3px 20px;
		line-height: 1.42857143;
		&:hover, &:focus {
			background-color: transparent;
			color: $nav-dropdown-link-color;
		}
	}
	> .active > a {
		background-color: transparent;
		color: $nav-dropdown-link-color;
		&:hover, &:focus {
			background-color: transparent;
			color: $nav-dropdown-link-color;
		}
	}
}
@media (max-width: 991px) {
	.header-main {
		.m-search-icon i {
			color: $main-color-text; 
			font-size: 20px;
			cursor: pointer;
			&:last-child {
				display: none;
			}  
		} 
		&.search-mobile-active {
			.m-search-icon i {
				&:first-child {
					display: none;
				}
				&:last-child { 
					display: block;
				}
			}
		}
		.m-cart-icon { 
			text-align: center;
			@include margin-start(20px);
			&.waiting {
				background: transparent !important;
			}
			.mobile-basket {
				line-height: 40px;
				max-height: 40px;
				position: relative;
				display: block;
				color: $main-color-text; 
				> .demo-icon {
					font-size: 20px; 
					color: $main-color-text;
					margin: 0;
					@include margin-end(10px);
				}
				> img {
					max-width: 20px;
				}
				.number {
					position: relative;
					font-weight: 700; 
				}
				.n-item {
					@include icons-number;
					background: $text-header-color;
					color: $bg-header-color;
					top: auto;
					right: auto;
					bottom: -10px;
					left: -25px;
				}
			}
		}
	}
	.navbar-responsive-menu {
		border: none;
		margin: 0;
		padding: 0 !important;
		min-height: 50px;
		width: 50px;
		height: 50px;
		cursor: pointer;
		z-index: 2; 
		.responsive-menu {
			width: 100%;
			height: 50px;
			line-height: 50px;
			z-index: 9;
			cursor: pointer;
			font-size: 0;
			&:before {
				content: '\f008';
				font-family: $font-family-icon;
				color: $main-color-text;
				font-size: $font-icon;
			}
		}
		.bar {
			display: none;
		}
	}
	.product-image-inner > .col-md-10 {
		width: 80%;
	}
	.sb-wrapper {
		margin: 0;
		background: none;
		border: none;
		height: 100%;
		overflow: auto;
		.sb-widget {
			margin: 0 auto; 
		}
	}
	.header-menu-wrapper {
		display: none;
	}
	table {
		thead th {
			padding: 10px 15px;
			&.price {
				padding: 10px 15px;
			}
		}
		td {
			padding: 15px;
		}
	}
	.cart-table table th {
		padding: 10px 15px;
	}
	.left-column-container #related_item {
		margin-top: 40px;
	}
}
@media (max-width: 479px) {
	.header-main .m-cart-icon .mobile-basket {
		> .demo-icon {
			margin: 0;
		}
		.number {
			font-size: 0;
			.money {
				display: none;
			}
		}
		.n-item {
			left: -11px;
			width: 18px;
			height: 18px;
			line-height: 18px; 
		}
	}
}
/*=== footer.scss ===*/
.footer-col:nth-child(2n+1) {
