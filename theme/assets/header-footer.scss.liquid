@mixin swatch-product {
    .swatch {
        margin: 15px 0 0;
        position: relative;
        .header {
            display: block;
            margin: 0;
            line-height: 34px;
            color: $body-title-color;
            float: left;
            min-width: 80px;
            padding: 0;
            @include padding-end(10px); 
        }
        .swatch-element {
            margin: 0;
            padding: 0;
            float: left;
            label {
                min-width: $color_swatch_width !important;
                height: $color_swatch_height !important;
                line-height: $color_swatch_height - 3px;
                position: relative;
                text-align: center;
                color: $body-title-color;
                font-size: $body-font-size - 1px;
                word-break: break-word;
                white-space: normal;
                font-weight: 700;
                text-transform: none;
                cursor: pointer;
                background-color: transparent;
                background-repeat: no-repeat !important;
                background-position: center !important;
                background-size: cover;
                border: 1px solid $body-border-color;
                margin: 0 0 5px;
                @include margin-end(5px); 
                @include border-radius-reset;
                @include box-shadow(0 0 0 0 #ffffff inset);
                &:before {
                    @include ab(0, 0, n, n);
                    width: 100%;
                    height: 100%;
                    cursor: pointer;
                }
            }
            input:checked + label {
                box-shadow: none;
                cursor: default;
                border: 2px solid $main-color;
                color: $main-color;
                @include box-shadow(0 0 0 0 #ffffff inset);
            }
            &.circle label {
                @include border-radius-circle;
                &:before {
                    @include border-radius-circle;
                }
            }
            &.img-true label {
                min-width: $image_swatch_width !important;
                height: $image_swatch_height !important;
            }
            &:hover {
                @media (min-width: 1025px) {
                    label {
                        border: 2px solid $main-color;
                        color: $main-color;
                        @include box-shadow(0 0 0 0 #ffffff inset);
                    }
                }
            }
            &.soldout {
                label {
                    cursor: default;
                    background-color: transparent !important;
                    color: $body-title-color !important;
                    border-color: $body-border-color !important;
                    &:before {
                        cursor: default;
                    }
                }
                &:hover {
                    @media (min-width: 1025px) {
                        label {
                            cursor: default;
                            background-color: transparent !important;
                            color: $main-color;
                            border: 1px solid $body-border-color;
                            &:before {
                                cursor: default;
                            }
                        }
                    }
                }
            }
            &.colour:not(.soldout) {
                input:checked + label {
                    cursor: default;
                    border: 2px solid $main-color;
                    &:before {
                        cursor: default;
                    }
                }
                &:hover {
                    @media (min-width: 1025px) {
                        label {
                            border: 2px solid $main-color;
                        }
                    }
                }
            }
        }
        &:last-child {
            margin-bottom: 15px;
        }
    }
}
@mixin drawer-sidebar {
    position: fixed;
    z-index: 999999;
    left: 0;
    top: 0;
    height: 100%;
    width: 300px;
    padding: 50px 20px 30px;
    overflow-y: auto;
    background: $body-background-color;
    @include opacity(0); 
    @include translate(-300px, 0);
    @include transition(all 0.45s cubic-bezier(0.25, 0.46, 0.45, 0.94));
    @media (max-width: 300px) {
        width: 100%;
        padding-bottom: 20px;
    }
    .f-close,
    .fof-close {
        text-align: center;
        cursor: pointer;
        background: $body-second-color;
        @include width-btn;
        @include ab(10px, n, n, 10px);
        @include border-radius-circle;
        @include transition(all 0.35s ease 0s);
        &:hover {
            background: $main-color;
            color: $main-color-text;
        }
    }
}
@mixin icons-number {
    @include ab(n, n, -9px, -9px);
    width: 22px;
    height: 22px;
    line-height: 22px;
    background: $bg-number-icon;
    color: $text-number-icon;
    text-align: center;
    font-size: 12px;
    font-weight: 700;
    @include border-radius-circle;
}
@mixin hover-icons-number {
    -webkit-animation-name: hvr-icon-bob-float, hvr-icon-bob;
    animation-name: hvr-icon-bob-float, hvr-icon-bob;
    -webkit-animation-duration: .3s, .75s;
    animation-duration: .3s, 0.75s;
    -webkit-animation-delay: 0s, .3s;
    animation-delay: 0s, .3s;
    -webkit-animation-timing-function: ease-out, ease-in-out;
    animation-timing-function: ease-out, ease-in-out;
    -webkit-animation-iteration-count: 1, infinite;
    animation-iteration-count: 1, infinite;
    -webkit-animation-fill-mode: forwards;
    animation-fill-mode: forwards;
    -webkit-animation-direction: normal, alternate;
    animation-direction: normal, alternate;
}
@mixin padding-carousel {
    .owl-stage-outer {
        padding: 0 0 35px;
        margin-bottom: -35px;
        @media (max-width: 1024px) {
            padding: 0;
            margin-bottom: 0;
        }
    }
    .owl-nav {
        top: -52px;
    }
}
@mixin middle-button-carousel ($sFont, $left, $right) {
    .owl-nav {
        position: static;
        button {
            @include ab(50%, n, n, n);
            @include translate(0, -50%);
            span:before {
                font-size: $sFont;
            } 
            &.owl-prev {
                left: 0;
                @media (min-width: 1500px) {
                    left: $left;
                }
            }
            &.owl-next {
                right: 0;
                @media (min-width: 1500px) {
                    right: $right;
                }
            }
        }
    }
}
@mixin title-wrapper {
    padding-bottom: 11px;
    line-height: 32px;
    position: relative;
    &:before {
        content: '';
        width: 100%;
        height: 2px;
        background: $main-color;
        @include ab(n, 0, -1px, 0);
    }
}
@mixin dropdown-effect {
    visibility: hidden;
    z-index: -999;
    display: block;
    @include opacity(0);
    @include transition(opacity 0.3s ease-in-out);
}
@mixin dropdown-effect-hover {
    @include opacity(1);
    visibility: visible;
    z-index: 9999; 
}
@mixin clearfix() {
    &:before, &:after {
        content: ""; 
        display: block;
        width: 100%;
        clear: both;
    }
}
@mixin is-countdown-separate($size, $lheight, $height) {
    &:after {
        content: ':';
        color: $body-title-color;
        font-size: $size;
        font-weight: $font-bold;
        line-height: $lheight;
        height: $height;
        @include ab(0, n, n, -4px);
    }
    &:first-child {
        @include padding-start(0);
    }
    &:last-child {
        @include padding-end(0);
        &:after {
            content: none;
        }
    }
}
@mixin carousel-loading() {
    height: 0;
    @include opacity(0);
    &.owl-loaded {
        @include opacity(1);
        height: auto;
    } 
}
@mixin width-boxed { 
    padding: 0 15px;
    margin: 0 auto; 
    width: 100%;  
    @media (min-width: 576px) {
        max-width: 100%; 
    } 
    @media (min-width: 768px) {
        max-width: 760px;
    }
    @media (min-width: 992px) {
        max-width: 980px;
    }
    @media (min-width: 1200px) {
        max-width: 1200px;
    }  
    @media (min-width: 1440px) {
        max-width: 1440px;
    }
}
				
/* header ----------------*/
html .cart-opened,
html.search-opened,
html .filter-opened {
	overflow: hidden;
}
body.cart-opened,
body.search-opened,
.sidebar-opened,
.order-sidebar-opened {
	#page-body:before {
		content: "" !important;
		@include ab(0, 0, 0, 0);
		background: rgba(0, 0, 0, 0.3);
		display: inline-block !important;
		z-index: 999991;
	} 
}
body.menu-opened { 
	overflow: hidden;
	#body-content:before {
		content: "" !important; 
		@include ab(0, 0, 0, 0);
		background: rgba(0, 0, 0, 0.3);
		display: inline-block !important;
		z-index: 999991;
	}  
}
.off-canvas-active {
	overflow: hidden;
	#page-body {
		@include transform(translateX(250px));
	} 
	#body-content:before {
		content: "" !important; 
		@include ab(0, 0, 0, 0);
		background: rgba(37,41,51,.2);
		display: inline-block !important;
		z-index: 999991;
	} 
} 
@media screen and (-webkit-min-device-pixel-ratio: 0) {
	::i-block-chrome,
	.cart-dd,
	.header-content .header-icons > div.searchbox .dropdown-menu,
	.header-content .header-main .table-row .header-menu-wrapper .header-dropdown-menu,
	.dropdown-menu > li.dropdown-submenu > .dropdown-menu,
	.horizontal-menu .navigation .navbar .navbar-collapse .main-nav > li.dropdown > .dropdown-menu,
	.cata-toolbar .show-by .dropdown-menu,
	.cata-toolbar .sort-by #sort_by_box {
		@include transition(opacity 0.35s ease);
	}
}
.top-bar {
	background: $topbar-bg-color;
	border-bottom: 1px solid rgba($text-header-color, 0.1);
	position: relative; 
	> div {
		position: relative;
	}  
	.multi-store-text {
		font-size: $top-font-size; 
		text-align: end; 
		@include ab(50%, -88px, n, n);   
		@include translate(0, -50%); 
		@media (max-width: 1900px) {
			display: none;
		}
		.icon {
			@include ab(8px, n, n, -15px); 
			&:after { 
				content: "";
				display: inline-block;
				border-top: 10px solid transparent;  
				border-bottom: 10px solid transparent;
