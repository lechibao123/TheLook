body {
	&.as-default-theme, &.as-none {
		div.snize-ac-results {
			display: none !important;
		}
	}
}
.header-content {
	padding: 0;
	margin: 0;
	position: relative;
	background: $bg-header-color;
	@media (max-width: 991px) {
		background: transparent;
	}
	.layout-full {
		margin: 0 auto;
	}
	.multi-store {
		@include margin-end(30px);  	
		@media (max-width: 1199px) {
			display: none; 
		}  
		ul li {
			display: inline-block;     
			padding: 0 !important;      //conflict css top bar header style-8
			@include padding-start(7px !important); //conflict css top bar header style-8
			&:before {
				content: none !important;     //conflict css top bar header style-8 
			}  
			&:first-child {
				@include padding-start(0); 
			}
			.inner {
				display: block;
				padding: 5px 12px;   
				font-size: $top-font-size + 1px; 
				@include border-radius(5px, 5px, 0px, 0px);
			} 
			img {
				border: 1px solid $body-border-color; 
				@include margin-end(5px);
				@include border-radius-circle;
			}
		}
	}   
	.header-container {
		@media (max-width: 991px) {
			background: $header-bg-mobile-color; 
			@include box-shadow(0 0 8px rgba(#c9c9c9, 0.75));
		}
		&.layout-boxed.style-2 {
			.header-navigation {
				background: none;
				margin: 0 auto;
				.table-row {
					background: $main-color;
					@include border-radius-10;
					.vertical-menu .navigation .head {
						@include border-radius(10px, 0, 0, 10px);
						&.opened {
							@include border-radius(10px, 0, 0, 0);
						}
					}
				}
			}
			.header-icons {
				@include padding-end(15px);
			}
		} 
		/*stick menu*/
		&.header-fixed {
			@media (min-width: 1025px) {
				.container {
					@include padding-start(15px !important);
					@include padding-end(15px !important);
				}
			}
			.header-product {
				display: none;
			}
			.header-main {
				position: fixed;
				z-index: 999;
				left: 0;
				right: 0;
				top: 0;
				background: $bg-header-color;
				width: 100%;
				max-width: 100%;
				-webkit-animation-name: fade-In-Down;
				animation-name: fade-In-Down;
				@include box-shadow(0 0 15px rgba($black, 0.65));
				@media (min-width: 992px) {
					padding-top: 10px !important;
					padding-bottom: 10px !important;
				}
				@media (max-width: 991px) {
					position: static;
					background: none;
				}
			}
			.vertical-menu .navigation .navbar:not(.opened) {
				@include opacity(0.001 !important);
				visibility: hidden !important;
			}
			&.style-1 {
				@media (min-width: 992px) {
					min-height: 194px;
				}
			}
			&.style-2 {
				@media (min-width: 992px) {
					min-height: 190px;
				}
			}
			&.style-3 {
				@media (min-width: 992px) {
					min-height: 193px;
				}
			}
			&.style-4 {
				@media (min-width: 992px) {
					min-height: 183px;
				}
			}
			&.style-5 {
				@media (min-width: 992px) {
					min-height: 104px;
					.header-main {
						background: $main-color;
					}
				}
			}
			&.style-6 {
				@media (min-width: 992px) {
					min-height: 115px;
					.header-main {
						background: $main-color;
					}
				}
			}
			&.style-7 {
				@media (min-width: 992px) {
					min-height: 121px;
				}
				.header-main .vertical-menu .sidemenu-holder .navigation .navbar {
					margin-top: 10px; 
				} 
			}
			&.style-8 {
				@media (min-width: 992px) {
					min-height: 125px;   
				} 
				&.header-fixed .off-canvas-navigation-wrapper {
					display: none; 
				}
			}
		}
		&.header-mobile-fixed {
			@media (max-width: 991px) {
				min-height: 60px;
				.header-main {
					background: $main-color;
					z-index: 999;
					width: 100%;
					top: 0;
					left: 0;
					position: fixed;
					-webkit-animation-name: fade-In-Down;
					animation-name: fade-In-Down;
				}
				+ .searchbox-mobile {
					position: fixed;
					top: 70px; 
				}
			}
		}
	}
	.header-main {
		position: relative;
		margin: 0 auto;
		padding: 32px 0;
		-webkit-animation-duration: .5s;
		animation-duration: .5s;
		-webkit-animation-fill-mode: both;
		animation-fill-mode: both;
		@media (max-width: 991px) {
			padding: 10px 0 !important; 
			margin: 0 auto;
		}
		.table-row {
			position: relative;
			display: flex;
			align-items: center;
			@media (min-width: 992px) {
				display: table;
				width: 100%;
			}
		}
	}
	.table-row {
		@media (min-width: 1200px) {
			.header-logo {
				width: 24.3%;
			}
		}
		@media (max-width: 1199px) and (min-width: 992px) {
			.header-logo {
				width: 24.1%;
			}
		}
		@media (min-width: 992px) {
			.header-logo {
				display: table-cell;
				vertical-align: middle;
				@include padding-end(15px);
			}
			.searchbox {
				display: table-cell !important;
				vertical-align: middle;
				width: 50%;
				padding: 0 15px;
			}
			.header-icons {
				display: table-cell !important;
				text-align: end;
				vertical-align: middle;
				width: 25%;
				@include padding-start(15px);
			}
		}
	}
	.searchbox {
		margin: 0;
		display: inline-block !important;
		position: relative;
		@media (max-width: 991px) {
			display: none !important;
		}
		&.searchbox-mobile {
			display: none !important; 
			@media (max-width: 991px) {
				display: block !important;
				padding: 15px 0;
				position: absolute;
				top: 100%;
				width: 100%;
				background: $body-background-color;  
				@include box-shadow(0 0 8px rgba(201,201,201,0.75));
				opacity: 0;
				visibility: hidden;
				height: 0;
				z-index: 999; 
				@include translate(0, 10px); 
				@include transition(transform 0.35s ease-in-out);
				&.active {
					opacity: 1;
					visibility: visible;
					height: auto; 
					@include translate(0, 0);  
				}
			}
			> div {
				position: relative;
			}
		}
		> a {
			display: block;
			width: 100%;
			text-align: center;
			position: relative;
			z-index: 991;
		}
		.dropdown-menu {
			left: auto;
			right: 0;
			width: 300px;
			display: block;
			z-index: -99; 
			visibility: hidden;
			@include opacity(0);
			@include translate(0, 10px);
			@include transition(all 0.35s ease);
			@media (max-width: 400px) {
				width: 280px;
				right: -60px;
			}
		}
		&:hover {
			@media (min-width: 1025px) {
				.dropdown-menu {
					visibility: visible;
					z-index: 99;
					@include opacity(1);
					@include translate(0, 0);
					&:before {
						content: '';
						height: 30px;
						width: 100%;
						@include ab(-20px, 0, n, n);
					}
				}
			}
		}
		&.show .dropdown-menu {
			visibility: visible;
			z-index: 99;
			@include opacity(1);
			@include translate(0, 0);
		} 
		.search-icon {
			@include ab(-2px, n, n, -3px);
			z-index: 3;
			width: 60px;
			min-width: 60px;
			height: 42px;
