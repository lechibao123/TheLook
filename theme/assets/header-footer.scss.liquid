.navbar {
	min-height: 10px;
	border: none;
	margin: 0;
	padding: 0;
	-webkit-box-align: baseline;
	-ms-flex-align: baseline;
	align-items: baseline;
	.bar {
		height: 3px;
		background: $nav-link-color;
		margin: 0 0 5px;
		display: block;
		@include border-radius(3px, 3px, 3px, 3px);
		&:nth-child(1) {
			width: 30px;
		}
		&:nth-child(2) {
			width: 24px;
		}
		&:nth-child(3) {
			width: 18px;
		}
	}
	&.navbar-responsive-menu {
		font-size: 0;
	}
}
.templateIndex {
	.header-navigation .table-row .vertical-menu .navigation .head {
		@include border-radius(10px, 10px, 0, 0);
		&.opened {
			@include border-radius-10;
		}
	}
	.header-content {
		.style-1, .style-2 {
			.header-navigation .table-row .vertical-menu .sidemenu-holder {
				.navbar {
					@include dropdown-effect-hover;
					display: block;
					z-index: 999;       //fix home 2
					&.opened {
						@include dropdown-effect;
						display: none;
					}
				}
			}
		}
		.header-container.layout-boxed.style-2 .header-navigation .table-row .vertical-menu .head {
			@include border-radius(10px, 0, 0, 0);
			&.opened {
				@include border-radius(10px, 0, 0, 10px);
			}
		}
		.style-5 {
			@media (min-width: 992px) {
				margin-bottom: 25px;
			}
		}
		.style-7 {
			&:not(.header-fixed) .header-main .vertical-menu .sidemenu-holder .navbar {
				display: block;
				@include opacity(1);
				visibility: visible;
				max-width: 270px;
				&.opened {
					display: none;
					@include opacity(0);
					visibility: hidden;
				}
			} 
		}
		.style-8 .header-navigation .table-row .vertical-menu .head.opened {
			@include border-radius(10px, 10px, 0, 0);
		}
	}
}
/*menu on PC---------------------------------*/  
.horizontal-menu {
	background: none; 
	position: static;
	.sidemenu-holder .menu-list > li {
		&.highlight, &.hover.highlight {
			> .dropdown-inner > a, > .dropdown-inner >.expand, > a, >.expand {
				color: $body-price-sale-color;
			}  
		}
		> .dropdown-menu {
			top: 100%; 
			left: 0;  
		}
		&.mega-menu {
			position: static;
			> .dropdown-menu { 
				&.position-center {
					left: 50%;
					@include translate(-50%, 10px);
				}
				&.position-right {
					left: auto;
					right: 0;
				}
				.dropdown-menu {
					font-size: $nav-sub-font-size;
				}
				&.column-4 {
					width: 100%;
				}
				&.column-3 {
					width: 75%;
				}
				&.column-2 {
					width: 50%;
				}
				&.column-1 {
					width: 25%;
				}
			}
		}
		&.mega-is-hover { 
			@include border-radius(0, 0, 0, 0);
			> .dropdown-inner > a, > a {
				color: $nav-link-hover-color;
			}
			> .dropdown-inner > span.expand,
			> span.expand {
				color: $nav-link-hover-color;
			}
			> .dropdown-menu {
				background: $nav-dropdown-background;
				@include dropdown-effect-hover;  
			} 
			&.mega-menu {
				> a {
					position: relative;
				} 
				> .dropdown-menu.position-center {
					@include translate(-50%, 0);
				} 
			}
		}
	} 
