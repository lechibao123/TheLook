        &.col-md-12 {
            position: relative;
            @media (max-width: 767px) {
                min-height: 10px !important;
            }
            .custom-item-inner {  
                width: 100%;  
                padding: 0;         
                @media (min-width: 768px) {
                    height: 100%;
                    width: 100%; 
                    @include ab(50%, 50%, n, n);   
                    @include translate(-50%, -50%);
                }          
                &.custom-item-inner-text {
                    .custom-block-heading-text {
                        margin: 0 auto;
                    }
