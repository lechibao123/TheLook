                    @include padding-start(2px);
                    +.hp-tabs-list { 
                        float: left; 
                        padding: 0; 
                        @include padding-end(1px);      
                    }   
                }  
            } 
        }
        .block-image {
            position: relative;
            display: block;
            .btn-link {
                padding: 9px 20px;
                @include ab(n, 50%, 20px, n);
                @include translate(-50%, 0); 
            } 
        }
        &.no-padding {     
            .hp-tabs-special  {
                @media (max-width: 991px) {
                    margin-bottom: 0;
                } 
            }
            .row { 
                margin: 0; 
                .product-grid-item {
                    padding: 0;
                    margin-bottom: 0;
                }   
            }
        }
        .hp-tabs-special {
            padding:0 2px; 
            @media (max-width: 767px) {
                padding: 0 calc(15px / 2); 
            }  
        }
        .hp-tabs-product {
            padding: 0;
        }    
        .hp-tabs-special  {
            @media (max-width: 991px) {
                margin-bottom: 4px;
            } 
            @media (max-width: 767px) {
                margin-bottom: 15px;
            } 
        }
        .row { 
            margin: 0 -2px; 
            @media (max-width: 767px) {
                margin: 0 calc(-15px / 2); 
            } 
            .block-product {
                margin: 0;
            }  
            .product-grid-item {
                padding: 0px 2px;
                margin-bottom: 4px;
                @media (max-width: 767px) {
                    padding: 0 calc(15px / 2);
                    margin-bottom: 15px;
                }
            }   
        } 
        .special-product-true:not(.special-product-v2) {
            .product-wrapper:before {
                content: none;
            }    
        }
        .block-special-product .price-cart-wrapper .product-add-cart {
            .btn-add-cart {
                width: auto;
                height: auto;
                padding: 0 16px;
                background: $button-hover-btn2;    
                color: $white;
                font-weight: $font-bold;        
                border: 1px solid $button-hover-btn2-border;
                @include border-radius-25;
                .text {
                    display: initial; 
                }
                @media (max-width: 1440px) {
                    padding: 0;
                    width: 35px;
                    height: 35px;
                    @include border-radius-circle;
                    .text {
                        display: none; 
                    }
                }  
                span {
                    @include margin-end(8px);
                    @media (max-width: 1440px) {
                        @include margin-end(0);
                    }
                }
                &:hover {
                    background: $button-hover-btn1;
                    color: $button-hover-btn1-txt;
                    border-color: $button-hover-btn1-border;
                }
            }
        }
    }
    &.tab-style-v2 {
        .nav-tab-wrapper {
            margin: 0 auto 6px;
        }
        .nav-tabs { 
            position: relative;
            text-align: center; 
            @media (min-width: 992px) {
                position: static;  
                display: block;
                @include translate(0, 0);
            }
            @media (min-width: 1200px) {    
                @include margin-start(calc(33.33333333% + 15px));
            }
            @media (max-width: 1199px) and (min-width: 992px) {
                @include margin-start(calc(41.6666666667% + 15px));
            }
            @media (max-width: 991px) and (min-width: 768px) {
                @include margin-start(calc(50% + 15px));
            }  
            li .nav-link {
                font-size: $size-h5;
            } 
        }
        .hp-tabs-special-product {
            @media (min-width: 1200px) {
                width: 33.33333333%;
            }
            @media (max-width: 1199px) and (min-width: 992px) {
                width: 41.6666666667%;
            }
            @media (max-width: 991px) and (min-width: 768px) {
                width: 45%;
            }
            @media (min-width: 768px) {   
                float: left;
                @include padding-end(15px);
            }            
            .product-wrapper {
                border: 2px solid $main-color;
                overflow: hidden;
                padding: 20px 20px 25px;           
                margin: -38px 0 0 0;   
                @include border-radius-20;  
                @media (max-width: 767px) {
                    margin: 0 0 15px;
                    padding: 20px 15px;
                }
                &:before,
                &:after {
                    content: none;
                }
                .product-head {
                    position: relative;
                    margin-bottom: 9px;             
                }
                .product-content { 
                    text-align: center;
                    .product-name {
                        margin: 0 0 12px;
                    }     
                    .product-price {
                        .price, .price-sale {
                            float: none;
                            font-size: $product-price-size + 10px;
                        }
                        .price-compare {
                            font-size: $body-font-size + 4px;
                        }
                    }  
                }
            }
        }
        .hp-tabs-list {
            @media (min-width: 1200px) {
                width: 66.66666667%;
            }
            @media (max-width: 1199px) and (min-width: 992px) {
                width: 58.3333333333%;
            }
            @media (max-width: 991px) and (min-width: 768px) {
                width: 55%;
            }
            @media (min-width: 768px) { 
                float: right; 
                clear: none;             
                @include padding-start(15px);
            } 
            .product-grid-item {
                @media (max-width: 767px) {
                    border-top: 1px solid $body-border-color; 
                }
                @media (max-width: 767px) and (min-width: 576px) {
                    &.col-sm-4:nth-child(-n+3) {
                        border-top: none;    
                    } 
                }
                @media (max-width: 575px) {
                    &.col-sm-4:nth-child(3n+1) {
                        .product-wrapper:before {
                            content: "";  
                        }
                    }
                    &.col-6 {
                        &:nth-child(2n+1) {
                            .product-wrapper:before {
                                content: none;   
                            }
                        }
                        &:nth-child(-n+2) {
                            border-top: none;   
                        }
                    }
                }  
            } 
        }
    } 
    &.tab-style-v3 {
        .product-wrapper {  
            display: table;
            table-layout: fixed;
            width: 100%;  
            height: 100%;      
            .product-head {
                display: table-cell;
                vertical-align: top;
                width: 43%;
                min-width: 100px;
                padding: 0;
                @include padding-end(15px);
                .featured-img {
                    margin: 0 auto; 
                    .product-label {
                        display: none;
                    }
                }
            }
            .product-content { 
                display: table-cell;
                vertical-align: top;
                width: 57%; 
                height: 100%;
                padding: 0; 
                @include padding-start(15px);
                .product-button {
                    text-align: start; 
                    padding: 0 15px 6px; 
                    width: 100%;  
                    @include padding-start(calc(43% + 15px));   
                }   
                .pc-inner {
                    height: 100%;
                    flex-direction: column;
                    @include flexbox;  
                }
                .price-cart-wrapper {
                    margin-top: auto;
                }
            }   
        }
        &.no-padding .owl-carousel {
            .product-grid-item .product-wrapper:nth-of-type(2) {
                margin-top: 0px;
            }    
        } 
        .tabs-list-carousel {
            @include carousel-loading();
        }
        .owl-carousel {
            @include middle-button-carousel ($font-small-icon + 3px, -18px, -18px);
            .product-grid-item .product-wrapper:nth-of-type(2) {
                margin-top: 30px;
            } 
        }
    } 
    &.tab-style-listing {
        .nav-tab-wrapper {
            margin: 0 !important;
            @media (min-width: 1200px) {
                width: 200px;
                float: left;  
                margin-top: 20px !important;        
            }  
            .nav {
                position: static;
                text-align: start;   
                border-bottom: none;  
                @include margin-start(0); 
                @media (max-width: 1024px) {
                    &:before {
                        content: none;
                    }
                }
                @media (max-width: 1199px) {          
                    padding: 0;
                    margin: 0 0 10px;
                    text-align: center;
                    border: none;
                }
                li {
                    display: block;
                    padding: 0;
                    margin: 0;
                    @media (max-width: 1199px) {
                        display: inline-block;
                    }
                    a {
                        display: block;
                        margin: 0;
                        padding: 7px 14px;
                        line-height: 25px;
                        font-size: $body-font-size;
                        border: none;
                        border-bottom: 1px solid $body-border-color;
                        color: $body-title-color;
                        text-align: start;
                        background: none;
                        @include border-radius-reset;
                        @media (max-width: 1199px) {
                            border-bottom: none;
                        }
                        &:after,
                        &:before {
                            content: none;
                        } 
                        &:hover {
                            @media (min-width: 1025px) {
                                font-weight: 700;
                            } 
                        }
                        &.show {
                            font-weight: 700;
                        }
                    }
                    &:last-child a {
                        border-bottom: none; 
                    } 
                }
                &.align-center {
                    width: 100%;
                    position: static;
                    @include translate(0, 0);
                }
            }     
        }
        .title-wrapper h3 {
            margin-bottom: 0;
        }     
        .wrap-tab-content .tab-content {
            padding-top: 0;
            @media (min-width: 1200px) {
                width: calc(100% - 200px);  
                float: left;  
                @include padding-start(20px);      
            } 
            .hp-tabs-list {
                display: block;
            }   
            .position-right {
                .hp-tabs-product {
                    @media (min-width: 768px) {
                        float: left;
                        padding: 0; 
                        @include padding-end(30px);
                    }     
                } 
                .hp-tabs-special {
                    @media (min-width: 768px) { 
                        float: right; 
                        padding: 0;      
                    }      
                }
            }
            .hp-tabs-product {
                width: 100%; 
                @media (min-width: 768px) {
                    width: 66.66667%;
                    float: right;
                    padding: 0;
                    @include padding-start(30px);
                }  
            }
            .hp-tabs-special {
                width: 100%;
                padding-bottom: 15px;
                @media (min-width: 768px) {
                    float: left;  
                    width: 33.33333%; 
                    @include padding-start(0); 
                }    
                &.hp-tabs-image {
                    padding-bottom: 30px;
                    @media (min-width: 1200px) {
                        margin-top: -15px;
                        padding-bottom: 0;
                    } 
                    @media (max-width: 767px) {
                        padding-bottom: 15px;
                    }   
                } 
            }
        } 
        &.no-padding .wrap-tab-content .tab-content {
            .hp-tabs-product {
                padding: 0;
            }
            .position-right .hp-tabs-product {
                padding: 0;
            }
            .hp-tabs-special .hp-tabs-image {
                padding-bottom: 0; 
            } 
            .hp-tabs-special {
                padding-bottom: 0;
            }  
        } 
        .block-special-product .wrap-thumb-image {
            padding: 4px 0;
        }  
        .price-cart-wrapper .product-add-cart .text {
            display: none;
        }
    }
    .fix-subheading-true .col-link {
        @media (max-width: 767px) {
            position: static;  
        } 
    }  
    .col-link { 
        display: inline-block;
        margin: 0 0 20px;
        @include ab(-104px, n, n, 0);  
        z-index: 1; 
        @media (min-width: 1025px) {
            top: -48px;  
        }          
        i {
            @include margin-start(5px); 
        }
    }
    /*=== tab-style-v1 - tab-style-listing ===*/
    .block-special-product {
        .product-wrapper { 
            .product-content {
                clear: both; 
            }
            &:before {
                content: none;
            }
        }
        .wrap-thumb-image {
            overflow: hidden;
            padding: 35px 0px 15px;  
            .thumb-image {
                float: left;
                width: 58px;
                padding: 0;
                border: 1px solid $body-border-color;
                position: relative;
                cursor: pointer; 
                margin: 0 0 7px;
                @include margin-end(7px); 
                &:before {
                    content: "";
                    height: 2px;
                    background: $main-color; 
                    @include opacity(0);
                    @include ab(n, 0px, -2px, 0px); 
                    @include transition(all 0.35s ease-in-out);
                }
                &:hover {
                    &:before {
                        @include opacity(1);
                    } 
                }
            }
        }
    }
    .tab-content {   
        position: relative; 
        @include clearfix();
        > .tab-pane {                     
            visibility: hidden;
            width: 100%;      
            @include opacity(0);  
            @include ab(0, 0, n, n);      
            @include transition(all 0.45s cubic-bezier(0.25, 0.46, 0.45, 0.94));      
            &.special_block-none { 
                display: block;
                float: left;
                width: 100%;
                clear: both;  
            }
            &.show {   
                @include opacity(1);        
                position: static;                 
                visibility: visible;        
            }  
        }
    } 
}
.hp-tabs-list {    
    clear: both;
    &.tabs-list-carousel {
        @include carousel-loading(); 
    }          
    &.owl-carousel {  
        @include padding-carousel;      
        @include middle-button-carousel($font-small-icon + 3px, -18px, -18px);    
    }    
    &.tabs-list-row {
        @include clearfix();  
    }    
}
/*=== product-grid-section ===*/
.product-grid-carousel {
    display: none;
}
.product-grid-section { 
    .product-listing-row,
    .product-grid-row {
        @media (max-width: 767px) {
            margin: calc(-15px / 2) !important;
        } 
        .product-grid-item {
            @media (max-width: 767px) {
                padding: calc(15px / 2) !important;
            } 
            .product-wrapper:before {
                @media (max-width: 575px) {
                    content: none; 
                } 
            }
        }
    } 
    .block-type-product-deal .title-wrapper {     	
        justify-content: left;      
        align-self: center;
        align-items: center;
        @include flexbox();
        @media (max-width: 575px) {
            display: block;
        }
        h3 {
            margin: 0;    
            @include margin-end(30px); 
        }
    }  
    .title-wrapper {  
        h3 {
            margin: 0; 
        }
        .header-countdown {  				
            background: $main-color;
            padding: 5px 25px;
            position: relative;
            bottom: 8px; 
            @include border-radius-25;
            @media (max-width: 575px) {
                margin: 13px 0;
                bottom: auto; 
            } 	 	  
            .countdown-title {
                display: inline-block;
                color: $main-color-text;
                font-size: $body-font-size + 1px;
                font-weight: $font-bold;
                line-height: 23px;
            }
            .wrapper-countdown {
                display: inline-block;
            }
            .is-countdown .countdown-section {  	
                width: auto;
                display: inline-block;
                padding: 0 5px;		
                position: relative; 
                &:last-child {
                    @include padding-end(0);
                } 
                &:not(:last-child) {
                    &:before {
                        content: ':';
                        color: $main-color-text;
                        font-size: $body-font-size + 1px;
                        font-weight: $font-bold;
                        line-height: 23px;
                        @include ab(0, n, n, -2px);	  					
                    }
                }
                .countdown-amount {
                    color: $main-color-text;
                    font-size: $body-font-size + 1px;
                    font-weight: $font-bold;
                    background: none;
                    line-height: 23px;
                }
                .countdown-period {
                    display: none;
                }
            }
        }
    }
    .product-listing-carousel  {
        @include carousel-loading();
    }  
    .owl-carousel { 
        @include padding-carousel;    
    } 
    .product-grid-content {
        padding-top: 34px;
        padding-bottom: 34px;
        background-repeat: no-repeat;
        background-position: center;
        background-size: cover;
        @media (max-width: 767px) {
            padding-top: 20px;
            padding-bottom: 20px;    
        }
    } 
    .block-type-product-half-width {
        .title-wrapper {
            margin-bottom: 25px; 
        }
        .owl-carousel .owl-nav {
            top: -65px;
        }
    }    
    .block-type-product-listing-hw {
        .title-wrapper {
            margin-bottom: 30px;
        } 
        .owl-carousel .owl-nav {
            top: -70px;
        }
    } 
    .block-type-product-half-width, 
    .block-type-product-listing-hw {
        padding-top: 47px;
        padding-bottom: 70px;
        @media (max-width: 767px) {
            padding-top: 20px;
            padding-bottom: 20px;
        }    
        .title-wrapper {
            @media (min-width: 768px) {
                @include margin-start(50%); 
            }    
            &:before {
                display: none;
            } 
        }
        .wrap-product-slider {
            margin: 0 auto;   
            @media (min-width: 768px) {
                @include margin-start(50%); 
            }    
            .product-wrapper:before {
                content: none; 
            }
            .product-grid-carousel .product-grid-item .product-wrapper:before {
                content: none; 
            }   
        }
    } 
    .block-type-product-listing {
        .title-wrapper  {
            margin-bottom: 24px;
        }
        .owl-carousel .owl-nav {
            top: -64px; 
        }
    } 
    .block-type-product-listing,
    .block-type-product-listing-hw {
        .row .product-wrapper {
            width: 100%;
        }     
        .product-grid-item.col-sm-6 {
            @media (max-width: 575px) {
                max-width: 100%; 
                @include flex(0 0 100%);
            } 
        }
        .product-wrapper {  
            display: table;
            table-layout: fixed; 
            height: 100%;      
            .product-head {
                display: table-cell;
                vertical-align: top;
                width: 43%;
                min-width: 100px;
                padding: 0;
                @include padding-end(15px);
                .featured-img {
                    margin: 0 auto;
                    .product-label {
                        display: none;
                    }
                }
            }
            .product-content { 
                display: table-cell;
                vertical-align: top;
                width: 57%; 
                height: 100%;
                padding: 0;
                @include padding-start(15px);
                .pc-inner {
                    height: 100%;
                    flex-direction: column;
                    @include flexbox;
                }
                .price-cart-wrapper {
                    margin-top: auto; 
                }
                .product-button {  
                    text-align: start; 
                    width: 100%;
                    padding: 0 15px 6px; 
                    @include padding-start(calc(43% + 15px));   
                }    
            } 
        }
    }
    .subheading-true.navigator-position-1 {
        .title-wrapper > a {
            @include padding-end(60px);
        }
    }
    .navigator-position-2 .owl-carousel {
        @include middle-button-carousel($font-small-icon + 3px, -18px, -18px);
    }
    .block-type-product-image {  
        .title-wrapper { 
            margin-bottom: 0;
        }
        .product-grid-inner {
            @include clearfix();
            margin: 0 -15px;
            .block-image {
                position: relative;
                display: block;
                .btn-link {
                    padding: 9px 20px;
                    width: fit-content;  
                    @include ab(n, 50%, 20px, n);
                    @include translate(-50%, 0); 
                }  
            } 
            .product-type-item {
                padding: 0 15px;
            }
            .product-grid-wrapper {
                padding: 0 15px;
            } 
        }
        .product-type-item { 
            width: 230px;
            float: left;  
            @media (max-width: 767px) {
                width: 100%;
                float: none;  
                margin-bottom: 15px;       
            }  
        } 
        .product-grid-wrapper {  
            width: calc(100% - 230px); 
            float: right; 
            @include padding-start(15px);
            @media (max-width: 767px) { 
                width: 100%;  
                clear: both;
                padding-top: 15px;
                @include padding-start(0);
            }  
        } 
    }
    .product-grid-inner.navigator-position-1 .block-type-product-image {
        @media (max-width: 767px) {
            .product-grid-inner {
                position: relative;
            } 
            .product-grid-carousel {
                position: static;
            }
            .owl-carousel .owl-nav {
                top: -42px;
            } 
        }
    } 
    .deal-position-2 {
        @include clearfix();
        @include flexbox();
        flex-wrap: wrap;
        margin: 0 -15px;  
        .product-type-item {
            padding: 0 15px;
            overflow: hidden;
            width: 100%;
            max-width: 100%;
            @include flex(0 0 100%);
            @media (min-width: 768px) {
                max-width: 33.3333333333%;
                @include flex(0 0 33.3333333333%);
            }
            @media (min-width: 992px) {
                max-width: 25%;
                @include flex(0 0 25%);
            }
            @media (min-width: 1200px) {
                max-width: 19.1666666667%; 
                @include flex(0 0 19.1666666667%);
            }
            .countdown-title {
                font-size: $size-h2 - 9px;
                font-weight: 400;
                line-height: 1.1;
                display: block;
                @include title-wrapper;
            } 
            .block-image {
                margin-top: 20px;
                display: block;
            }
            .wrapper-countdown {
                padding-top: 15px;
                overflow: hidden;
                .is-countdown {
                    background: none;
                    border: none;
                }
                .countdown-section {
                    width: auto;
                    padding: 8px 5px;
                    text-align: center;
                    background: $white;
                    border: 1px solid $main-color;
                    float: left;
                    margin: 0;
                    @include margin-end(2%); 
                    @include border-radius(5px, 5px, 5px, 5px);
                    span {
                        display: block;
                        &.countdown-amount {
                            font-size: 23px;              
                            color: $body-title-color;
                        }
                        &.countdown-period {
                            text-transform: uppercase;
                            font-size: 10px;
                            color: $body-title-color;
                        }
                    }
                    &:last-child {
                        @include margin-start(0); 
                    }
                }
            }
        }  
        .product-grid-wrapper {
            padding: 0 15px;
            width: 100%;
            max-width: 100%;
            @include flex(0 0 100%);
            @media (max-width: 767px) {
                padding-top: 15px; 
            } 
            @media (min-width: 768px) {
                max-width: 66.6666666667%;
                @include flex(0 0 66.6666666667%);
            }
            @media (min-width: 992px) {
                max-width: 75%;
                @include flex(0 0 75%);
            }
            @media (min-width: 1200px) {
                max-width: 80.8333333333%; 
                @include flex(0 0 80.8333333333%);
            }
        }
        .product-wrapper:before {
            content: none !important;
        } 
    }
}
.carousel {
    display: none;
}
/*=== list-collection-section ===*/
.list-collection-section {
    padding-top: 40px;
    padding-bottom: 40px;  
    @media (max-width: 767px) {
      padding-bottom: 20px;
    }  
    .title-wrapper {
        margin-bottom: 30px;
    }
}  
.list-collection { 
    @include flexbox;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;  
    &.border-true {
        margin: 0;
        .collection-item {
            padding: 0;
            margin: 0 0 35px; 
            @media (max-width: 767px) {
                margin-bottom: 15px;
            } 
            @media (max-width: 479px) {
                max-width: 100%;
                @include flex(100%); 
                .item {
                    border-top: 1px solid $body-border-color;
                    @include border-start(none); 
                }
                &:first-child .item {
                    border-top: none; 
                }
            }
            @media (min-width: 992px) {
                &.col-fix-5:nth-child(5n+1) {
                    .item {
                        @include border-start(none);
                    } 
                }
                &.col-lg-3:nth-child(4n+1) {
                    .item {
                        @include border-start(none);
                    } 
                }
                &.col-lg-4:nth-child(3n+1) {
                    .item {
                        @include border-start(none);
                    } 
                }
                &.col-lg-2:nth-child(2n+1) {
                    .item {
                        @include border-start(none);
                    }  
                }
            }
            @media (max-width: 991px) and (min-width: 768px) {
                &.col-md-3:nth-child(4n+1) {
                    .item {
                        @include border-start(none);
                    } 
                }
                &.col-md-4:nth-child(3n+1) {
                    .item {
                        @include border-start(none);
                    } 
                }
                &.col-md-6:nth-child(2n+1) {
                    .item {
                        @include border-start(none);
                    } 
                }
            }
            @media (max-width: 767px) {
                &.col-sm-4:nth-child(3n+1) {
                    .item {
                        @include border-start(none);
                    } 
                } 
                &.col-sm-6:nth-child(2n+1) {
                    .item {
                        @include border-start(none);
                    } 
                } 
                &.col-sm-12 .item {
                    @include border-start(none);
                } 
            }
        }
        .item {
            @include border-start(1px solid $body-border-color);
        }  
    }
    .collection-item {
        margin: 10px 0;
    }   
    .item {
        position: relative;                 
        background: $white;
        width: 100%;
        height: 100%;
        padding: 15px;
        align-items: center;
        @include flexbox;   
        @include transition(all 0.35s ease 0s); 
        &.sub-collections-true {
            vertical-align: top; 
            .image {
                vertical-align: top;
            } 
            .text-box {
                vertical-align: top;
            } 
        }
        .image {
            width: 45%;
            padding: 0; 
            @include padding-end(10px);
        } 
        .text-box {
            width: 55%; 
            @include padding-start(10px);     
            h6 {
                font-weight: 400;
                margin: 0 0 8px; 
                line-height: 1.3;
                font-size: $size-h6 + 2px; 
                a {
                    color: $body_title_color;
                    &:hover {
                        color: $main-color; 
                    }
                }
            }
            > a {
                font-weight: 700;
            }
            ul li {
                list-style: none;
                line-height: 21px; 
                a {
                    color: $body_light_color; 
                    font-size: $body-font-size - 1px;
                    &:hover {
                        color: $main-color; 
                    }
                }
            }
        }
        &:hover {
            @media (min-width: 1025px) {
                z-index: 6;  
                @include border-start(none);
                @include border-radius(5px, 5px, 5px, 5px);      
                @include box-shadow(0 0 5px 0 #c6c6c6); 
            }     
        }       
    }   
}
.p-carousel-banner-section {
    padding: 130px 0;
    @media (max-width: 767px) {
      padding: 80px 0; 
    }
    .block-product {
        .title {
            color: $main-color;
            font-family: $font-heading;
            line-height: 1.0;
            font-weight: 700;
        }
        .subtitle {
            font-family: $font-heading;      
        }
    }
    .product-carousel-content {
        margin: 50px 0 0;
        .product-wrapper {
            border: none;
            .product-content .product-name {
                font-size: $body-font-size;
            } 
        }
        .swatch-size-wrapper,
        .product-description,
        .product-price,
        .product-button {
            display: none;
        }
    }
}
/*=== policies-content ===*/ 
.policies-content {
    border: 1px solid $body-border-color;  
    @include border-radius-10;
    .policy-list {
        overflow: hidden;
        > .policy-item {
            float: left;
            @media (min-width: 992px) {
                width: 20%;
            }
            @media (max-width: 991px) and (min-width: 768px) {
                width: 33.333333%;
            }
            @media (max-width: 767px) and (min-width: 480px) {
                width: 50%;
            }
            @media (max-width: 479px) {
                width: 100%;
            }
            &:first-child {
                @include border-start(none);
            }
        } 
    }
    .owl-item {
        padding: 0;
        &:first-child {
            .policy-item {
                @include border-start(none);
            }
        } 
        .policy-item {
            @media (max-width: 479px) {
                @include border-start(none);
            }  
        } 
    }
    .policy-item {  
        padding: 23px 5px 19px; 
        text-align: center;    
        justify-content: center;
        -webkit-justify-content: center;
        align-items: center;
        -webkit-align-items: center;
        @include flexbox();
        @include border-start(1px solid $body-border-color);    
        .image {   
            display: inline-block;   
            width: 45px;       
        } 
        .text {      
            display: inline-block;
            padding: 0; 
            @include padding-start(10px);     
            h6 {        
                margin: 0 0 3px;        
                font-weight: 700;
                font-size: $body-font-size;
            }
            span {
                display: block;
                color: $body-title-color;
            }
        }
    }
} 
/*=== single-product-section ===*/
.single-product-carousel {
    display: none;
    &.owl-loaded {
        display: block;
    }     
}  
.single-product-section { 
    .title-wrapper {
        margin: 0 auto;
        border-bottom: none; 
        h3 {
            border: none;
            padding: 0;
            margin: 0 0 17px; 
            &:before {
                content: none;
            }  
        }   
    }
    .sp-item {
        background-size: cover;
        background-position: center;
        background-repeat: no-repeat;
    }
    .single-product-list {
        position: relative;
        margin: 0 auto 17px;
        @media (max-width: 767px) {
            margin-bottom: 10px;
        }
    }
    .product-wrapper { 
        .deal-progress { 
            clear: both;
            padding: 20px 0 0;
            .deal-stock {
                overflow: hidden;
                padding: 0 0 11px;
                text-align: start;
            }
            .stock-available {
                color: $body-title-color;
            }
            .stock-sold {
                float: right;
                color: $body-title-color;
            }
            .progress {
                margin: 0;
                background: #eee;
                height: 20px;
                border: none;
                @include border-radius-20;
                @include box-shadow(inset 0 0 0 rgba(0,0,0,.1));
            }
            .progress-bar {
                background: $main-color;
                line-height: 20px;
                @include border-radius-20; 
                @include box-shadow(inset 0 0 0 rgba(0,0,0,.1));
            }
        }
    } 
    .owl-carousel .owl-stage-outer {
        padding: 0; 
        margin-bottom: 0;  
    } 
    .full-width .style-1 {
        margin-bottom: 0; 
        .sp-item {
            .text-box {
                @media (min-width: 1200px) {
                    width: 35%;
                    @include padding-start(50px);
                }  
            }
            .product-wrapper {
                background: transparent; 
                @media (max-width: 991px) {
                    padding-top: 30px;
                }    
                @media (min-width: 1200px) {
                    width: 65% !important;
                    .product-head {
                        width: 66.66667%;
                    } 
                }
                .product-content {
                    @media (min-width: 1200px) {  
                        width: 33.33333%;
                    } 
                    .btn-buy-now { 
                        border: 2px solid $button-hover-btn1-border;  
                        padding: 6px 50px;
                        @include border-radius-10;  
                    }
                    .product-name {
                        font-size: $size-h5 - 1px; 
                        a {
                            color: $body-title-product;  
                        } 
                    }  
                }
            }
        }
    }
    .style-1 {
        .sp-item {
            padding: 30px 5%;
            @media (min-width: 992px) {
                display: table;
                width: 100%;
                table-layout: fixed;
            }     
            @media (max-width: 767px) {
                @include padding-start(0);
                @include padding-end(0);
            }
            .text-box {
                @media (min-width: 992px) {             
                    display: table-cell;
                    vertical-align: middle;
                    width: 33.333333%;
                    @include padding-end(15px);
                }
                .text-1 {
                    line-height: 1.0;
                    font-weight: 300;
                    font-size: 40px;
                    @media (min-width: 1400px) {
                        font-size: 48px;
                    }
                    @media (max-width: 1199px) and (min-width: 992px) { 
                        font-size: 29px;
                    }
                    @media (max-width: 479px) {
                        font-size: 30px;
                    }
                }
                .text-2 {
                    font-size: 64px;
                    font-weight: 300;
                    line-height: 0.8;
                    @media (min-width: 1400px) {
                        font-size: 4.8vw;
                    }
                    @media (max-width: 1199px) and (min-width: 992px) {
                        font-size: 45px;
                    }
                    @media (max-width: 479px) {
                        font-size: 50px;
                    }
                }
                .text-3 {
                    font-size: 15px;
                    font-weight: 700;
                    padding: 33px 0 0;
                }       
            }
            .product-wrapper {
                padding: 0;
                @media (min-width: 992px) {
                    float: none;
                    display: table-cell;
                    vertical-align: middle;
                    width: 66.66666% !important;
                    padding: 0;
                    background: none;
                    .product-wrapper-inner > div {
                        width: 50%;
                        display: table-cell;
                        vertical-align: middle;
                        float: none;
                    }
                    .product-head {
                        padding: 15px;
                    }
                    .product-content {
                        @include padding-start(15px);
                    }
                }
                @media (max-width: 991px) and (min-width: 768px) {
                    display: table;
                    width: 100%;
                    vertical-align: middle;
                }
                &:after,
                &:before {
                    content: none;
                }
                .product-wrapper-inner {
                    @media (min-width: 768px) {
                        display: table;
                        width: 100%;
                        vertical-align: middle;
                        table-layout: fixed;
                    }
                }
                .product-head {
                    @media (max-width: 991px) and (min-width: 768px) {
                        width: 50%;
                        display: table-cell;
                        vertical-align: middle;
                        float: none;
                        @include padding-end(15px);
                    }
                    .special-product-label {
                        display: none;
                    }
                }
                .product-content {
                    @media (max-width: 991px) and (min-width: 768px) {
                        width: 50%;
                        display: table-cell;
                        vertical-align: middle;
                        float: none;
                        @include padding-end(15px);   
                    }
                    .product-name {
                        margin: 0 0 12px;
                        font-size: $size-h5;
                    }
                    .product-price {
                        overflow: hidden;
                        margin: 0 0 24px;
                    }
                    .price-compare {
                        font-size: 17px;
                        float: left;
                        margin: 12px 0 0;
                        @include margin-end(15px); 
                    }
                    .price-sale,
                    .price {
                        font-size: 34px;
                        float: none;
                    }
                    .offer-text {
                        margin: 0 0 20px;
                        color: $body-title-color;   
                        display: none;      
                    }
                    .wrapper-countdown {
                        clear: both;
                        margin: 0 0 30px;
                        overflow: hidden;
                        .is-countdown {
                            border: none;
                            background: none;
                            white-space: nowrap;
                            padding: 0;
                            .countdown-section {
                                width: auto;
                                padding: 10px 13px;
                                text-align: center;
                                background: $white;
                                border: 1px solid $main-color;
                                float: left;
                                margin: 0;
                                @include margin-end(12px); 
                                @include border-radius(5px, 5px, 5px, 5px);
                                @media (max-width: 1199px) and (min-width: 768px) {
                                    @include padding-start(7px);
                                    @include padding-end(7px);
                                }
                                @media (max-width: 479px) {
                                    @include padding-start(8px);
                                    @include padding-end(8px);
                                }
                                span {
                                    display: block;
                                    float: none;
                                    &.countdown-amount {
                                        font-size: 25px;
                                        font-weight: 700;
                                        color: $body-title-color;
                                        @media (min-width: 1400px) {
                                            font-size: 2.4vw;
                                        }
                                        @media (max-width: 1199px) and (min-width: 992px) {
                                            font-size: 20px;
                                        }
                                        @media (max-width: 479px) {
                                            font-size: 8vw;
                                        }
                                    }
                                    &.countdown-period {
                                        text-transform: uppercase;
                                        font-size: 10px;
                                        color: $body-title-color;
                                        @media (min-width: 1400px) {
                                            font-size: 12px;
                                        }               
                                    }
                                }
                                &:last-child {
                                    @include margin-end(0); 
                                }
                            }
                        }
                    }
                }
            }
        }
        .owl-carousel {
            .owl-dots {
                white-space: nowrap;
                @include ab(n, 50%, 0, n);
                @include translate(-50%, 0);
                @media (max-width: 767px) {
                    bottom: 5px;
                } 
            }
            .owl-nav {
                display: none;
            } 
        }
    } 
    .style-2, .style-3, .style-4 {
        .product-wrapper {
            >div {
                align-items: center;
            }   
            .pc-inner {
                text-align: center;
                @media (max-width: 767px) {
                    padding: 15px 0 0;
                }  
                .product-name {
                    margin: 0 0 5px;
                }      
                .product-price {
                    display: inline-block;
                }
                .price-sale,
                .price {            
                    font-size: 30px;
                    float: none;
                }          
                .price-compare {
                    float: left;
                    font-size: $size-h5;
                    color: #383838; 
                    margin: 8px 0 0;
                    @include margin-end(10px); 
                }
                .offer-text {
                    display: block;           
                    color: #383838;
                    font-size: $body-font-size + 1px;
                    margin: 15px auto 0;
                }   
                .is-countdown {           
                    text-align: center;
                    margin: 9px auto 0;
                    overflow: hidden;
                    .countdown-row {
                        padding: 0;
                    } 
                    .countdown-section {
                        width: auto;
                        margin: 0 auto;
                        padding: 0 7px;  
                        display: inline-block;
                        position: relative;
                        float: none;
                        @media (max-width: 479px) {
                            padding: 0 4px;
                        }
                        @include is-countdown-separate(30px, 40px, 40px);
                        .countdown-amount {             
                            font-size: 25px; 
                            padding: 0 5px;
                            line-height: 42px;
                            float: none;
                            margin: 0 auto;              
                            background: $bg-deal-section;
                            color: $text-deal-text;
                            @include border-radius(5px, 5px, 5px, 5px);
                            @media (max-width: 479px) {
                                font-size: 7vw;
                                padding: 0 8px;
                            }
                        }
                        .countdown-period {
                            font-size: 12px;
                            text-transform: uppercase;
                            float: none;              
                            padding: 5px 0 0;
                            color: $body-title-color;
                        }
                    }
                }
            }
        }
        .owl-carousel .owl-nav {
            display: none;  
        } 
    } 
    .style-2 { 
        .product-wrapper {  
            border: 2px solid $main-color;          
            overflow: hidden;
            padding: 20px 23px;
            margin: 0;        
            @include border-radius-20;
            @media (max-width: 479px) {
                padding: 15px;
            }
            &:before,
            &:after {
                content: none;
            }
            .product-head {
                position: relative;       
                .wrap-image { 
                    @include padding-end(100px);
                    @media (max-width: 767px) {
                        @include padding-end(85px);
                    }  
                }
                .product-image {
                    overflow: hidden;
                }
                .special-product-label {    
                    width: auto;
                    height: auto;
                    padding: 10px 20px;
                    right: auto;
                    left: 0; 
                    @include border-radius-10;  
                    .text {
                        font-weight: 700;
                        font-size: $body-font-size;
                    }
                    .percent {
                        font-size: $body-font-size; 
                    }
                }       
            }
            .wrap-thumb-image {         
                width: 100px; 
                padding: 0 15px;  
                max-height: 100%;
                overflow-y: auto;
                @include ab(0, n, n, 0);
                @media (max-width: 767px) {
                    padding: 0;
                    width: 70px;
                }
                .thumb-image {
                    list-style: none;
                    display: block;
                    margin: 0 0 14px 0;
                    width: 100%;
                    position: relative;
                    cursor: pointer;
                    border: 1px solid $body-border-color;
                    &:after {
                        content: '';
                        height: 2px;
                        width: 100%;
                        @include ab(n, 0, -1px, 0);
                        @include transition(all 0.35s ease 0s);
                    }
                    &:hover {
                        @media (min-width: 1025px) {
                            &:after {
                                background: $main-color;
                            }
                        }
                    }
                    &:last-child {
                        margin-bottom: 0 !important;
                    }
                }
            } 
            .pc-inner {
                .offer-text {
                    margin-top: 28px;
                }
                .deal-progress {
                    padding-top: 29px;
                    .deal-stock {
                        padding-bottom: 13px;  
                    }
                } 
                .is-countdown {
                    margin-top: 17px;
                }
            }   
        }
    }
    .style-3 {
        > div {
            border: 2px solid $main-color;
            @include border-radius-20;
        } 
        .product-content {
            @media (min-width: 992px) {
                @include padding-start(0); 
            } 
        }
        .single-product-row { 
            margin: 0;
            .sp-item {
                padding: 0;
                &:first-child { 
                    .product-wrapper:before {
                        content: none;
                    }  
                }
            }
        } 
        .owl-item.current .product-wrapper:before {
            content: none;
        }  
        .product-wrapper {
            @include border-radius-20;  
            &:after {
                content: none;
            }
            &:hover {
                &:before {
                    content: "";
                }
            }
        }
    }
    .style-4 {
        .single-product-row {
            margin: 0 -10px;
            .sp-item {
                margin: 0 0 20px;
                padding: 0 10px;
            }    
        }  
        .product-content {
            @media (min-width: 992px) { 
                @include padding-start(0);
            } 
        } 
        .product-wrapper {
            border: 2px solid $main-color; 
            @include border-radius-20;
            &:before,
            &:after {
                content: none;
            }  
        }
    }  
}
.special-product-label {    
    @include ab(0, n, n, 0);    
    z-index: 1;
    background: $body-label-sale-background;
    color: $body-label-sale-color;
    padding: 19px 5px;
    line-height: 1.2;
    text-align: center;
    width: 76px;
    height: 76px;
    @include border-radius-circle;
    .text {
        display: block;
        font-size: 12px;
    }  
    .percent {
        display: block;
        font-size: $size-h4;
        font-weight: 700;
    }  
}
/*=== Customize Block ===*/
.custom-content-width {
    padding: 0 15px;
    .col-md-12 {    
        padding: 0;
        margin: 0 auto;   
    }
}
.custom-content-wrapper {
    &.custom-content-margin-top {
        margin-top: 30px;    
    }
    &.custom-content-margin-bottom {
        margin-bottom: 30px    
    }
