            .bc-item {
                flex-grow: 1; 
            }
            .bc-item-inner {
                position: relative;
                overflow: hidden; 
                height: 100%; 
                @include flexbox;
                align-items: center;
                padding: 14px 5px;  
                @include padding-end(20px);
                .bc-item-body {
                    margin: auto;
                    .bc-item-text {
                        font-weight: 200;
                        text-transform: uppercase;
                        font-size: $body-font-size + 3px;
                        a {
                            color: $body-general-color; 
                        }
                    }
                    > a {
                        font-size: $body-font-size;
                        line-height: 30px;
                        margin-top: 0;
                        position: relative;
                        font-weight: $font-bold; 
                        color: $body-general-color;
                        &:after {
                            @include prev-icon;
                            font-family: $font-family-icon;
                            font-size: 10px;
                            color: $white; 
                            width: 18px;
                            height: 18px;
                            text-align: center;
                            line-height: 18px; 
                            @include ab(0, n, n, -30px);
                            background-color: $main-color;
                            @include border-radius-circle;  
                        }
                        .upto {
                            text-transform: uppercase;
                            font-weight: 300;
                            display: inline-block;
                            .prefix {
                                display: inline-block;
                                width: 19px;
                                margin-bottom: 2px;
                                line-height: 1;
                                word-break: break-word;
                                @include margin-end(4px);
                            }
                            .value {
                                font-weight: 700;
                                font-size: 30px;
                            }
                            .suffix { 
                                position: relative;
                                font-weight: 700;
                                vertical-align: top; 
                                top: -3px;
                                @include margin-start(2px);
                            }
                        }
                    }
                }
            }  
            > a {
                display: block;
                text-align: center;
                margin: 0 0 12px;
                @media (max-width: 1024px) {
                    margin: 0;
                    width: 33.33333%;
                    padding: 0 10px;
                    float: left;
                } 
                &:last-child {
                    margin: 0;
                } 
                img {
                    width: 100%; 
                }
            } 
        } 
    }
} 
.bc-home-slideshow {  
    position: relative; 
    .swiper-container {
        height: 100%;
        &:before {
            content: "";
            @include ab(0, 0, n, n);
            width: 100%;
            height: 100%;
            background: #f5f5f5; 
        }
        &.swiper-container-initialized:before {
            content: none;
        } 
    } 
    .swiper-slide {
        position: relative;  
        height: auto;
        margin: 0 auto;   
        visibility: hidden;
        @include opacity(0);
        .img-mobile { 
            @media (min-width: 768px) {
                display: none;   
            }     
        }
        .img-desk { 
            @media (max-width: 767px) {
                display: none;
            }
        }
        &.has-video {
            .container-box {
                @include opacity(0);
            }     
            .video {
                min-width: 100%;
                min-height: 100%;    
                @include ab(50%, 50%, n, n);
                @include translate(-50%, -50%); 
            }     
            iframe {
                max-width: 100%;
                width: 100%;
                height: 100%;
            }
            &:hover .container-box {
                @include opacity(1);
            }
        }
        &.swiper-slide-active { 
            visibility: visible; 
            @include opacity(1);   
        }   
    }
    .swiper-content {
        @include ab(0, 0, 0, 0); 
        .container-box {
            position: absolute;
            z-index: 99;
        } 
    }
    &:hover .swiper-button-white {
        @media (min-width: 1025px) {
            @include opacity(1); 
        } 
    }  
    /*=== tab-wrapper ===*/
    .tab-wrapper {
        max-width: 1200px;
        padding: 0 15px;
        @media (min-width: 1200px) {        
            @include ab(n, 0, 0, 0);        
            margin: 0 auto;
        }   
        @media (max-width: 1199px) {
            margin-top: 15px;
            display: none;
        }   
        .tab-content {
            position: relative;
            clear: both;
            z-index: 2;
            .tab-pane {
                @include ab(0, 0, n, n);
                @include opacity(0);
                width: 100%;
                &.active {
                    position: static;
                    @include opacity(1);
                }
            }
            .slideshow-tabs-list {
                margin-bottom: 50px;  
            }
            .owl-stage-outer {
                margin-bottom: 0;
                padding-bottom: 0;
            }
            .owl-stage {            
                padding-top: 2px;   
                .owl-item .product-wrapper {
                    width: 100%;
                    float: none;
                    @include margin-end(0); 
                }        
            }
            .product-wrapper {
                width: 158px;
                float: left;
                @include margin-end(10px);
                &:before {
                    content: none;
                }
                &:after {
                    bottom: 0;
                }
                .product-head .product-name {
                    line-height: 19px;
                    overflow: hidden;
                    display: -webkit-box;
                    display: -ms-box;
                    -webkit-line-clamp: 2;
                    -webkit-box-orient: vertical;
                    margin: 0 0 10px;
                } 
                .product-content .pc-inner .price-cart-wrapper {
                    border: none;
                    padding-bottom: 0;
                }
                &:hover .product-content .product-button {
                    display: none;
                } 
            }
            .owl-carousel {
                @include middle-button-carousel ($font-small-icon + 3px, -38px, -38px); 
            }
        }
    }
    .nav-tab-wrapper {  
        z-index: 2;                     
        width: 200px;
        background: $body-background-color;
        float: right;
        margin: 0 0 3%; 
        @include border-radius(5px, 5px, 5px, 5px);
        .nav {          
            border: none;
            padding: 0;
            overflow: initial;  
            &:before {
                content: none;
            }  
        } 
        .nav-item {
            display: block;
            margin: 0;                  
            a {
                display: block;
                text-align: start;
                line-height: 22px;
                padding: 7px 25px;              
                font-size: $body-font-size;             
                &:before {
                    content: none;
                }
                &:after {
                    content: '';
                    width: 26px;
                    height: 26px;
                    background: transparent;
                    @include ab(5px, -13px, auto, auto);
                    @include rotate(45);
                    @include border-radius-reset;
                    @include transition(all 0.35s ease 0s);
                }
                &:hover {                   
                    font-weight: $font-bold;
                }
                &.active {
                    background: $main-color;
                    color: $main-color-text;
                    &:after {
                        background: $main-color;
                    }
                }
            }
            &:first-child a.active {
                @include border-radius(0, 5px, 0, 0);
            }       
            &:last-child a.active {
                @include border-radius(0, 0, 5px, 0);
            } 
        }   
    }
    .image-layer-box {
        @include ab(0%, n, n, n); 
        height: 100%;  
        @media (max-width: 767px) {
            display: none;
        } 
        .image-layer-slide {
            height: 100%;  
            position: relative;  
            -webkit-animation-duration: 1s;
            animation-duration: 1s;
            -webkit-animation-fill-mode: backwards;
            -moz-animation-fill-mode: backwards;
            -ms-animation-fill-mode: backwards;
            -o-animation-fill-mode: backwards;
            animation-fill-mode: backwards;
            -webkit-animation-iteration-count: 1;
            animation-iteration-count: 1; 
        }   
        .image-lazysize { 
            position: static !important;
            padding: 0 !important;
            height: 100%;
            img {
                position: static; 
                object-fit: contain;
                max-width: 600px;
                height: auto;
            }  
        }  
    } 
}
